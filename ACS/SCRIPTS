// 718
Script "CoDC4" (void) NET
{
	GiveInventory("C4DetonatorCheck", 1); 
	GiveInventory("C4Activekey", 1); 
	ActivatorSound("c4/click", 127);
}

// Fire Select Icon
// 994
Script "CoDFireSelectIcon" (int Which)
{
	if(Which == 1) //Full
	{
		SetResultValue(1);
		SetFont("IIRE_FA");
	    SetHudSize(1920, 1080, 1);
		HudMessage(s:"A"; HUDMSG_PLAIN, 824, CR_Green, 1700.0, 950.0, 0, 0.25);
		
	}
	else if(Which == 2) //2
	{
		SetResultValue(1);
