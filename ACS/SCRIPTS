// Class/Tip
// 709
Script "CoDTip" (int Which)
{
	if(Which == 1) //LONE SURVIVOR
	{
		SetResultValue(1);
		SetFont("CLASS_1");
	    SetHudSize(1920, 1080, 1);
		TakeInventory("bd_classicmonsters", 1);
		TakeInventory("bd_TraditionalMode", 1);
	    HudMessage(s:"A"; HUDMSG_FADEINOUT, 888, CR_Green, 300.0, 300.0, 11.5, 0.25);
		
	}
	else if(Which == 2) //UAC
	{
