Script "CoDBWInitialize" ENTER
{
	TakeInventory("NightVisionActive", 1);
	TakeInventory("Called_Price", 1);
	TakeInventory("Called_Woods", 1);

	if (CheckInventory("Called_Akid"))
	{
		Spawn("Marine_Akid_A", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
	}

	if (CheckInventory("Called_Iris"))
	{
		Spawn("Marine_Iris_A", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
	}

	if (CheckInventory("Called_Sakuya"))
	{
		Spawn("Marine_Sakuya_A", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
	}

	if (CheckInventory("Called_China"))
	{
		Spawn("Marine_China_A", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
	}

	if (CheckInventory("Called_Maiko"))
	{
		Spawn("Marine_Maiko_A", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
	}

	if (CheckInventory("Called_Meryl"))
	{
		Spawn("Marine_Meryl_A", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
	}
}

// 713
Script "CoDBOSelectFire" (void) NET
{
	GiveInventory("BOSelectFire", 1); 
}

