// Fire Select
// 894
Script "CoDFireSelect" (int Which)
{
	if(Which == 1) //Full
	{
		SetResultValue(1);
		SetFont("FIRE_FA");
	    SetHudSize(1920, 1080, 1);
	    HudMessage(s:"A"; HUDMSG_FADEINOUT, 878, CR_Green, 1700.0, 950.0, 1.0, 0.25);
		
	}
	else if(Which == 2) //2
	{
		SetResultValue(1);
		SetFont("FIRE_2");
	    SetHudSize(1920, 1080, 1);
	    HudMessage(s:"A"; HUDMSG_FADEINOUT, 878, CR_Green, 1700.0, 950.0, 1.0, 0.25);

