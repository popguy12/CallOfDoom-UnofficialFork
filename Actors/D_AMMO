ACTOR CoDAmmoPickup : CustomInventory
{
  Inventory.PickupSound "ammopiup"
  +INVENTORY.AUTOACTIVATE
  +DONTGIB
}

//-------------------------- .44mag
ACTOR MagnumAmmo1 : CoDAmmoPickup 
{
  Inventory.PickupMessage ".44 JSP Magnuforce Ammo +6"
  Scale 0.14
  States
  {
  Spawn:
    AMMR J -1
    Stop
  Pickup:
	BKPK A 0 A_GiveInventory("Ammo44Mag", 6)
    Stop
  }
}

