actor TacticalInventory : CustomInventory
{
  inventory.pickupmessage "Tactical Insertion"
  inventory.pickupsound "tactical/insert"
  Inventory.MaxAmount 3
  Scale 0.5
  +INVENTORY.INVBAR
  inventory.respawntics 2100
  states
  {
  Spawn:
    BROK A -1 
    stop
  Use:
    TNT1 A 0 A_GiveInventory("TacticalInsertion", 1)
    TNT1 A 0 A_SelectWeapon("TacticalInsertion") 
    stop
  }
}

actor TacticalInventory2 : TacticalInventory
{
  +INVENTORY.AUTOACTIVATE
  inventory.amount 0
}

actor Select_Friends : Inventory
{
    inventory.amount 0
    inventory.maxamount 20
}

actor ECAmmo1 : Ammo
{
    inventory.amount 1
    inventory.maxamount 5
    ammo.backpackamount 1
    ammo.backpackmaxamount 10
    +IGNORESKILL
}

