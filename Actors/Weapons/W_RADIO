actor UACRadioPickup : CustomInventory
{
  inventory.pickupmessage "UAC Satellite Phone"  
  inventory.pickupsound "RADIOCALL"
  +INVENTORY.AUTOACTIVATE
  +DONTGIB
  scale 0.14
  States
  {
  Spawn:
    AMMR W 10 BRIGHT A_SpawnItem("BlueFlareSmall", 0, 7)
	AMMR W 10 BRIGHT
    loop
  Pickup:
    GUNP A 0
    GUNP A 0 A_JumpIfInventory("TacticalInsertion", 1, "AmmoPickup") // If the player already has this weapon, they get more ammo for it
    GUNP A 0 A_GiveInventory("TacticalInsertion", 1)
	GUNP A 0 A_GiveInventory("ECAmmo1", 1)
	GUNP A 0 ACS_NamedExecuteWithResult("CoDNewWeapon", 43, 0, 0)
	stop
  AmmoPickup:
    GUNP A 0
    GUNP A 0 A_JumpIfInventory("ECAmmo1", 0, "FailPickup")
    GUNP A 0 A_GiveInventory("ECAmmo1", 1)
    stop
  FailPickup:
    AMMR W 1
    fail
  FailPickup2:
    AMMR W 1 
    GUNP A 0 ACS_ExecuteAlways(620, 0, 2, 0, 0)
    fail
  }
}

actor TacticalInsertion : RGUWeapon
{
  weapon.selectionorder 667
  Tag "Satellite Phone"
  weapon.ammotype1 "ECAmmo1"
  weapon.ammouse1 1
  weapon.ammogive1 1
  inventory.pickupsound "RADIOCALL"
  inventory.pickupmessage "UAC Satellite Phone"  
  +NOAUTOFIRE
  +INVENTORY.UNDROPPABLE
  States
  {
  Spawn:
	AMMR W 10 BRIGHT A_SpawnItem("BlueFlareSmall", 0, 7)
	AMMR W 10 BRIGHT
	loop
  Select:
    TNT1 A 1 A_Raise
    TNT1 AA 0 A_Raise
    loop
  Ready:
    TNT1 A 0 A_TakeInventory("Weight")
    TNT1 A 0 A_GiveInventory("Weight")
    TNT1 A 0 ACS_NamedExecuteAlways("CoDPlayerSpeed", 0, 0, 0, 0)
    TNT1 A 0 ACS_NamedExecuteWithResult("CoDWeaponLook", 56, 0, 0)
    TNT1 A 0 ACS_NamedExecuteWithResult("CoDFireSelectIcon", 99, 0, 0)
    TNT1 A 0 ACS_NamedExecuteWithResult("CoDScopeSelectIcon", 99, 0, 0)
	TNT1 A 0 ACS_NamedExecuteWithResult("CoDWeaponIcon", 43, 0, 0)
	TNT1 A 0 A_TakeInventory("BOSelectFire", 1)
	TNT1 A 0 A_TakeInventory("BOSelectScope", 1)
	TNT1 A 0 A_TakeInventory("BOWeaponlook", 1)
	TNT1 A 0 A_TakeInventory("BOWeaponlooking", 1)
	TNT1 A 0 A_TakeInventory("Reloading", 1)
	TNT1 A 0 A_TakeInventory("Aiming", 1)
	TNT1 A 0 A_PlayWeaponSound("weapon/select")
    PHON ABCDE 2 A_WeaponReady(WRF_NOFIRE)
	TNT1 A 0 A_PlayWeaponSound("M4A1/reload2")
	PHON FGHIJKLM 2 A_WeaponReady(WRF_NOFIRE)
	TNT1 A 0 A_PlayWeaponSound("RADIOSET2")
	PHON NOP 2 A_WeaponReady(WRF_NOFIRE)
	TNT1 A 0 A_PlayWeaponSound("RADIOSET2")
	PHON QRS 2 A_WeaponReady(WRF_NOFIRE)
	TNT1 A 0 A_PlayWeaponSound("M4A1/reload3")
	PHON TUVW 2 A_WeaponReady(WRF_NOFIRE)
    goto InHand	
  Ready2:
    TNT1 A 0 //A_GiveInventory("Weight", 1)
	TNT1 A 0 ACS_NamedExecuteAlways("CoDPlayerSpeed", 0, 0, 0, 0)
	TNT1 A 0 A_PlayWeaponSound("mp412/select")
    PHON ZYX 2 A_WeaponReady(WRF_NOFIRE)
    goto InHand
  InHand: 		
  Friend1S: //MBOX
	TNT1 A 0 A_TakeInventory("Select_Friends", 0)//Reset
	TNT1 A 0 A_TakeInventory("BOSelectScope", 1)
	TNT1 A 0 A_TakeInventory("BOSelectFire", 1)
	TNT1 A 0 A_GiveInventory("Select_Friends", 1)//1
	TNT1 A 0 //A_PlayWeaponSound("RADIOSET2")
	TNT1 A 0
	TNT1 A 0 ACS_NamedExecuteWithResult("CoDFriendSelect", 1, 0, 0)
	TNT1 A 0 A_JumpIfInventory("BOSelectScope", 1, "Friend2")//down
	TNT1 A 0 A_JumpIfInventory("BOSelectFire", 1, "Friend14")//up
	PHON W 1 A_WeaponReady
    goto Friend1S+5
  Friend1: //MBOX
	TNT1 A 0 A_TakeInventory("Select_Friends", 0)//Reset
	TNT1 A 0 A_TakeInventory("BOSelectScope", 1)
	TNT1 A 0 A_TakeInventory("BOSelectFire", 1)
	TNT1 A 0 A_GiveInventory("Select_Friends", 1)//1
	TNT1 A 0 A_PlayWeaponSound("RADIOSET2")
	TNT1 A 0
	TNT1 A 0 ACS_NamedExecuteWithResult("CoDFriendSelect", 1, 0, 0)
	TNT1 A 0 A_JumpIfInventory("BOSelectScope", 1, "Friend2")//down
	TNT1 A 0 A_JumpIfInventory("BOSelectFire", 1, "Friend14")//up
	PHON W 1 A_WeaponReady
    goto Friend1+5
	
  Friend2: //Medikit
	TNT1 A 0 A_TakeInventory("Select_Friends", 0)//Reset
	TNT1 A 0 A_TakeInventory("BOSelectScope", 1)
	TNT1 A 0 A_TakeInventory("BOSelectFire", 1)
	TNT1 A 0 A_GiveInventory("Select_Friends", 2)//2
	TNT1 A 0 A_PlayWeaponSound("RADIOSET2")
	TNT1 A 0
	TNT1 A 0 ACS_NamedExecuteWithResult("CoDFriendSelect", 2, 0, 0)
	TNT1 A 0 A_JumpIfInventory("BOSelectScope", 1, "Friend3")//down
	TNT1 A 0 A_JumpIfInventory("BOSelectFire", 1, "Friend1")//up
	PHON W 1 A_WeaponReady
    goto Friend2+5
  Friend2S: //Medikit
	TNT1 A 0 A_TakeInventory("Select_Friends", 0)//Reset
	TNT1 A 0 A_TakeInventory("BOSelectScope", 1)
	TNT1 A 0 A_TakeInventory("BOSelectFire", 1)
