	TNT1 A 0 A_TakeInventory("BOSelectScope", 1)
	TNT1 A 0 A_TakeInventory("BOSelectFire", 1)
	TNT1 A 0 A_GiveInventory("Select_Friends", 14)
	TNT1 A 0 //A_PlayWeaponSound("RADIOSET2")
	TNT1 A 0
	TNT1 A 0 ACS_NamedExecuteWithResult("CoDFriendSelect", 14, 0, 0)
	TNT1 A 0 A_JumpIfInventory("BOSelectScope", 1, "Friend1")//down
	TNT1 A 0 A_JumpIfInventory("BOSelectFire", 1, "Friend13")//up
	PHON W 1 A_WeaponReady
    goto Friend14S+5

  Deselect:
    TNT1 A 0 A_TakeInventory("Weight", 0)	//Determines player speed - 1 means normal speed
    TNT1 A 0 A_TakeInventory("Aiming", 1)
	TNT1 A 0 A_TakeInventory("KnifeAttack", 1)
	FRGA I 0 A_TakeInventory("ThrowingGrenade", 1)
	TNT1 A 0 ACS_NamedExecuteWithResult("CoDTip", 9, 0, 0)
	TNT1 A 0 ACS_NamedExecuteWithResult("CoDFriendSelect", 15, 0, 0)
    TNT1 A 0 ACS_NamedExecuteAlways("CoDPlayerSpeed", 0, 0, 0, 0)
    TNT1 A 0 A_TakeInventory("Reloading", 1)
    TNT1 A 0 A_ZoomFactor(1.0)
    TNT1 A 0 A_SetCrosshair(0)
    PHON XYZ 2
  DeselectFull: 				//Deselect
    TNT1 AA 0 A_Lower
    TNT1 A 0 A_Lower
    loop

  Fire:
    TNT1 A 0
    TNT1 A 0 A_JumpIfInventory("ECAmmo1", 1, "CALLfriend")
    TNT1 A 0
  RadioERROR:
	TNT1 A 0
	TNT1 A 0 A_PlaySoundEx("RADIOERR", "SoundSlot6")
  RadioReset:
    TNT1 A 0
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 14, "Friend14S")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 13, "Friend13S")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 12, "Friend12S")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 11, "Friend11S")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 10, "Friend10S")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 9, "Friend9S")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 8, "Friend8S")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 7, "Friend7S")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 6, "Friend6S")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 5, "Friend5S")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 4, "Friend4S")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 3, "Friend3S")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 2, "Friend2S")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 1, "Friend1S")
	TNT1 A 0
	goto Friend1S
	
  CALLfriend:
	TNT1 A 0
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 14, "CallFriend14")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 13, "CallFriend13")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 12, "CallFriend12")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 11, "CallFriend11")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 10, "CallFriend10")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 9, "CallFriend9")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 8, "CallFriend8")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 7, "CallFriend7")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 6, "CallFriend6")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 5, "CallFriend5")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 4, "CallFriend4")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 3, "CallFriend3")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 2, "CallFriend2")
	TNT1 A 0 A_JumpIfInventory("Select_Friends", 1, "CallFriend1")
	TNT1 A 0
	goto CallFriend1
	
  CallFriend1:
    TNT1 A 0
    TNT1 A 0 //A_JumpIfInventory("Called_SWAT", 1, "RadioERROR")
	TNT1 A 0 
	TNT1 A 0 A_PlayWeaponSound("RADIOCALL")
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_MBOX", 1, 1, 0, 0)
	TNT1 A 0 
	TNT1 A 0 //A_GiveInventory("Called_SWAT", 1)
	TNT1 A 0 
	goto RadioReset
	
  CallFriend2:
    TNT1 A 0
    TNT1 A 0 //A_JumpIfInventory("Called_SWAT", 1, "RadioERROR")
	TNT1 A 0 
	TNT1 A 0 A_PlayWeaponSound("RADIOCALL")
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_Medikit", 20, 1, 0, 0)
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_Medikit", -20, 0, 0, 0)
	TNT1 A 0 
	TNT1 A 0 //A_GiveInventory("Called_SWAT", 1)
	TNT1 A 0 
	goto RadioReset
	
  CallFriend3:
    TNT1 A 0
    TNT1 A 0 //A_JumpIfInventory("Called_SWAT", 1, "RadioERROR")
	TNT1 A 0 
	TNT1 A 0 A_PlayWeaponSound("RADIOCALL")
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_Armor", 1, 1, 0, 0)
	TNT1 A 0 
	TNT1 A 0 //A_GiveInventory("Called_SWAT", 1)
	TNT1 A 0 
	goto RadioReset
	
  CallFriend4:
    TNT1 A 0
    TNT1 A 0 //A_JumpIfInventory("Called_SWAT", 1, "RadioERROR")
	TNT1 A 0 
	TNT1 A 0 A_PlayWeaponSound("RADIOCALL")
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_556", 1, 1, 0, 0)
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_9mm", 10, 0, 0, 0)
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_45ACP", -10, 0, 0, 0)
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_PDW", 20, 0, 0, 0)
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_12G", -20, 0, 0, 0)
	TNT1 A 0 
	TNT1 A 0 //A_GiveInventory("Called_SWAT", 1)
	TNT1 A 0 
	goto RadioReset
	
  CallFriend5:
    TNT1 A 0
    TNT1 A 0 //A_JumpIfInventory("Called_SWAT", 1, "RadioERROR")
	TNT1 A 0 
	TNT1 A 0 A_PlayWeaponSound("RADIOCALL")
    TNT1 A 0 A_FireCustomMissile("FriendProjectile_Grenade", 20, 1, 0, 0)
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_M72LAW", -20, 0, 0, 0)
	TNT1 A 0 
	TNT1 A 0 //A_GiveInventory("Called_SWAT", 1)
	TNT1 A 0 
	goto RadioReset
	
  CallFriend6:
    TNT1 A 0
    TNT1 A 0 //A_JumpIfInventory("Called_SWAT", 1, "RadioERROR")
	TNT1 A 0 
	TNT1 A 0 A_PlayWeaponSound("RADIOCALL")
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_marine1", 1, 1, 0, 0)
    TNT1 A 0 A_FireCustomMissile("FriendProjectile_marine2", 20, 0, 0, 0)
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_marine3", -20, 0, 0, 0)
	TNT1 A 0 
	TNT1 A 0 //A_GiveInventory("Called_SWAT", 1)
	TNT1 A 0 
	goto RadioReset
	
  CallFriend7:
    TNT1 A 0
    TNT1 A 0 A_JumpIfInventory("Called_Price", 1, "RadioERROR")
	TNT1 A 0 
	TNT1 A 0 A_PlayWeaponSound("RADIOCALL")
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_Price", 1, 1, 0, 0)
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_Soap", 20, 0, 0, 0)
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_Ghost", -20, 0, 0, 0)
	TNT1 A 0 
	TNT1 A 0 A_GiveInventory("Called_Price", 1)
	TNT1 A 0 
	goto RadioReset
	
  CallFriend8:
    TNT1 A 0
    TNT1 A 0 A_JumpIfInventory("Called_Woods", 1, "RadioERROR")
	TNT1 A 0 
	TNT1 A 0 A_PlayWeaponSound("RADIOCALL")
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_Woods", 1, 1, 0, 0)
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_Hudson", 20, 0, 0, 0)
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_Reznov", -20, 0, 0, 0)
	TNT1 A 0 
	TNT1 A 0 A_GiveInventory("Called_Woods", 1)
	TNT1 A 0 
	goto RadioReset
	
  CallFriend9:
    TNT1 A 0
    TNT1 A 0 A_JumpIfInventory("Called_Akid", 1, "RadioERROR")
	TNT1 A 0 
	TNT1 A 0 A_PlayWeaponSound("RADIOCALL")
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_Akid", 1, 1, 0, 0)
	TNT1 A 0 
	TNT1 A 0 A_GiveInventory("Called_Akid", 1)
	TNT1 A 0 
	goto RadioReset
	
  CallFriend10:
    TNT1 A 0
    TNT1 A 0 A_JumpIfInventory("Called_Iris", 1, "RadioERROR")
	TNT1 A 0 
	TNT1 A 0 A_PlayWeaponSound("RADIOCALL")
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_Iris", 1, 1, 0, 0)
	TNT1 A 0 
	TNT1 A 0 A_GiveInventory("Called_Iris", 1)
	TNT1 A 0 
	goto RadioReset
	
  CallFriend11:
    TNT1 A 0
    TNT1 A 0 A_JumpIfInventory("Called_Sakuya", 1, "RadioERROR")
	TNT1 A 0 
	TNT1 A 0 A_PlayWeaponSound("RADIOCALL")
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_Sakuya", 1, 1, 0, 0)
	TNT1 A 0 
	TNT1 A 0 A_GiveInventory("Called_Sakuya", 1)
	TNT1 A 0 
	goto RadioReset
	
  CallFriend12:
    TNT1 A 0
    TNT1 A 0 A_JumpIfInventory("Called_China", 1, "RadioERROR")
	TNT1 A 0 
	TNT1 A 0 A_PlayWeaponSound("RADIOCALL")
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_China", 1, 1, 0, 0)
	TNT1 A 0 
	TNT1 A 0 A_GiveInventory("Called_China", 1)
	TNT1 A 0 
	goto RadioReset
	
  CallFriend13:
    TNT1 A 0
    TNT1 A 0 A_JumpIfInventory("Called_Maiko", 1, "RadioERROR")
	TNT1 A 0 
	TNT1 A 0 A_PlayWeaponSound("RADIOCALL")
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_Maiko", 1, 1, 0, 0)
	TNT1 A 0 
	TNT1 A 0 A_GiveInventory("Called_Maiko", 1)
	TNT1 A 0 
	goto RadioReset
	
  CallFriend14:
    TNT1 A 0
    TNT1 A 0 A_JumpIfInventory("Called_Meryl", 1, "RadioERROR")
	TNT1 A 0 
	TNT1 A 0 A_PlayWeaponSound("RADIOCALL")
	TNT1 A 0 A_FireCustomMissile("FriendProjectile_Meryl", 1, 1, 0, 0)
	TNT1 A 0 
	TNT1 A 0 A_GiveInventory("Called_Meryl", 1)
	TNT1 A 0 
	goto RadioReset
	
  KnifeAttack:
    TNT1 A 0 A_ZoomFactor(1.0)
    TNT1 A 0 A_SetCrosshair(0)
	TNT1 A 0 A_GiveInventory("Followcall", 1)
    TNT1 A 0 A_TakeInventory("Aiming", 1)
    TNT1 A 0 A_TakeInventory("Reloading", 1)
    TNT1 A 0 ACS_NamedExecuteAlways("CoDPlayerSpeed", 0, 0, 0, 0)
    TNT1 A 0 A_JumpIfInventory("ThrowingGrenade", 1, "FragGrenade")
    TNT1 A 0 ACS_NamedExecuteAlways("CoDRecoil", 0, 900, 900, 1)		//Recoil script
	TNT1 A 0 A_PlaySound("NEEDCOME")
    KNI9 A 1 A_AlertMonsters
    KNI9 BC 1
    KNI9 D 1 A_FireBullets(0.0, 0.0, 1, 40, "MachineGunBulletPuff", 0, 80)
    KNI9 EFGHIJ 1
    TNT1 A 10
	TNT1 A 0 A_TakeInventory("Followcall", 1)
    TNT1 A 0 A_TakeInventory("KnifeAttack", 1)
    goto Ready2+2
  FragGrenade:
    FRGA I 0 A_TakeInventory("KnifeAttack", 1)
	TNT1 A 0 A_TakeInventory("Followcall", 1)
    TNT1 A 0 A_JumpIfInventory("ThrowingStunGrenade", 1, "StunGrenade")
	TNT1 A 0 A_PlaySound("NEEDAMMO")
	TNT1 A 0 A_GiveInventory("Ammocall", 1)
	PHON C 40 
	FRGB I 0 A_TakeInventory("KnifeAttack", 1)
    FRGA I 0 A_TakeInventory("ThrowingGrenade", 1)
	TNT1 A 0 A_TakeInventory("Ammocall", 1)
    goto InHand
  Flashbang:
    FRGA ABCDE 3
    FRGA F 3 A_PlaySoundEx("flash/pinpull", "SoundSlot5")
    TNT1 A 15 
    FRGB AB 3
    FRGB H 0 A_PlaySoundEx("flash/throw", "SoundSlot5")
    FRGB I 0 A_TakeInventory("ThrowingGrenade", 1)
    FRGB I 0 A_TakeInventory("KnifeAttack", 1)
    FRGB I 0 A_TakeInventory("ThrowingFlashbang", 1)
    FRGB C 3 A_FireCustomMissile("FlashbangThrown", 0, 0, 0, 16)
    FRGB DEFG 3 
    goto Ready2+1		//Reselect weapon
  StunGrenade:
    TNT1 A 0 A_GiveInventory("StunGrenadeInventory", 1)
	TNT1 A 0 A_PlaySound("NEEDSTIM")
	TNT1 A 0 A_GiveInventory("Stimcall", 1)
	PHON C 0
    FRGB I 0 A_TakeInventory("ThrowingGrenade", 1)
    FRGB I 0 A_TakeInventory("KnifeAttack", 1)
    FRGB I 0 A_TakeInventory("ThrowingStunGrenade", 1)
    goto InHand	
  }
}

actor Followcall : Inventory
{
    inventory.amount 0
    inventory.maxamount 1
}

actor Ammocall : Inventory
{
    inventory.amount 0
    inventory.maxamount 1
}

actor Stimcall : Inventory
{
    inventory.amount 0
    inventory.maxamount 1
}

actor GivingAmmo : Inventory
{
    inventory.amount 0
    inventory.maxamount 5
}

actor Helicall : Inventory
{
    inventory.amount 0
    inventory.maxamount 1
}

actor FriendProjectile_MBOX
{
	Radius 16
	Height 40
	Speed 4
	Damage 0
                   Scale 0.5
        +NoBlockmap
        +ActivatePCross
        +ActivateImpact
        -NoTeleport
        +DropOff
		  +CANBOUNCEWATER
        +DontFall
        +Missile
         +DOOMBOUNCE
          +BOUNCEONACTORS
		  +DONTSPLASH
          DeathSound "misc/teleport"
	States
	{
	Spawn:
		TNT1 A 1 Bright
		Loop
	Death:
		TNT1 A 0 
		TNT1 A 0 //A_SpawnItemEx ("MBOX_spawn", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		TNT1 A 0 A_SpawnItemEx ("Backpack", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		goto death2
	Death2:
		TNT1 A 0 A_SpawnItem("TeleportFog_COD")
		Stop
	}
}

