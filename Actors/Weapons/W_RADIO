actor rgafriends
{

Obituary	"%o has been gunned down by a Marine."
DamageType	"Friendry"
damagefactor "Friendry", 	0.0
DamageFactor "CauseObjectsToSplash", 0.0
damagefactor "TeleportRemover", 0.0
damagefactor "GibRemoving", 0.0
 +FRIENDLY
}

ACTOR bia_gunflash : rgafriends
{
	Radius 16
	Height 16
	Speed 15
	Damage 0
	scale 1
	Projectile 
	+RANDOMIZE
	+NOCLIP
	RenderStyle Add
	Alpha 0.6
	States
	{
	Spawn:
		TNT1 A 2
		MUZB A 0 A_Jump(256, "p1", "p2", "p3", "p4", "p5", "p6", "p7", "p8")
		stop
	p1:
      MUZC A 1 bright 
	  stop
	p2:
      MUZC B 1 bright 
	  stop
	p3:
      MUZC C 1 bright 
	  stop
	p4:
      MUZC D 1 bright 
	  stop
	p5:
      MUZC E 1 bright 
	  stop
	p6:
      MUZC F 1 bright 
	  stop
	p7:
      MUZC G 1 bright 
	  stop
	p8:
      MUZC H 1 bright 
	  stop
	}
}

