ACTOR Beowulf_Pickup : CustomInventory
{
  inventory.pickupmessage "[8] Alexander Arms Beowulf .44 Magnum semi-automatic rifle"
  inventory.pickupsound "nothing"
  +INVENTORY.AUTOACTIVATE
  +DONTGIB
  inventory.respawntics 35
  scale 0.16
  States
  {
  Spawn:
    B004 K -1
    Stop
  Pickup:
    GNEX A 0
	GUNP A 0 A_JumpIfInventory("Beowulf", 1, "failPickup")  //Do you have this?
	GUNP A 0 A_JumpIfInventory("Mulekick", 1, 2)         //Are you using unlimited mode?
	GUNP A 0 A_JumpIfInventory("gun3SV", 0, "notPickup") //Do you have space in your inventory?
	GUNP A 0 A_GiveInventory("gun3SV", 1) 
	TNT1 A 0 A_PlaySound("misc/w_pkup")      //Pick up!
	GUNP A 0 A_GiveInventory("Beowulf", 1)      //weapon
	GUNP A 0 A_GiveInventory("BeowulfClip", 25) //clip
    TNT1 A 0 A_TakeInventory("BeowulfEmpty", 1) //no reload
	GNEX A 0 ACS_NamedExecuteWithResult("CoDNewWeapon", 84, 0, 0)
	GUNP A 0 //A_SelectWeapon("Beowulf")
    Stop
  FailPickup:
    B004 K -1
    fail
  notPickup:
    TNT1 A 0 ACS_NamedExecuteWithResult("CoDNotPickup", 84, 0, 0)
    B004 K -1
    fail
  }
}

ACTOR Beowulf_PickupE : CustomInventory
{
  inventory.pickupmessage "[8] Alexander Arms Beowulf .44 Magnum semi-automatic rifle"
  inventory.pickupsound "nothing"
  +INVENTORY.AUTOACTIVATE
  +DONTGIB
  inventory.respawntics 35
  scale 0.16
  States
  {
  Spawn:
    B004 K -1
    Stop
  Pickup:
    GNEX A 0
	GUNP A 0 A_JumpIfInventory("Beowulf", 1, "failPickup")  //Do you have this?
	GUNP A 0 A_JumpIfInventory("Mulekick", 1, 2)         //Are you using unlimited mode?
	GUNP A 0 A_JumpIfInventory("gun3SV", 0, "notPickup") //Do you have space in your inventory?
	GUNP A 0 A_GiveInventory("gun3SV", 1) 
	TNT1 A 0 A_PlaySound("misc/w_pkup")      //Pick up!
	GUNP A 0 A_GiveInventory("Beowulf", 1)      //weapon
	//GUNP A 0 A_GiveInventory("BeowulfClip", 5) //clip
    //TNT1 A 0 A_TakeInventory("BeowulfEmpty", 1) //no reload
	GNEX A 0 ACS_NamedExecuteWithResult("CoDNewWeapon", 84, 0, 0)
	GUNP A 0 //A_SelectWeapon("Beowulf")
    Stop
  FailPickup:
    B004 K -1
    fail
  notPickup:
    TNT1 A 0 ACS_NamedExecuteWithResult("CoDNotPickup", 84, 0, 0)
    B004 K -1
    fail
  }
}

ACTOR RemoveBeowulf: Marine1Pathfinder //unload
{
-FRIENDLY
-SHOOTABLE
Scale 1.0
Speed 1
MaxTargetRange 90000
	States
	{
	Spawn:
	Pain:
    See:
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 0)
		TNT1 A 0 Thing_ChangeTID(0, 98)
		TNT1 AAAAAAAAAAAAAAAAA 1 A_Chase
		//TNT1 A 0 Thing_Hate(98, 264, 0)
		TNT1 A 0 A_Recoil(-5)
		TNT1 A 1 A_FaceTarget
		Loop
	Missile:
		TNT1 A 1
		TNT1 A 0 A_JumpIfInTargetInventory("Ammo44mag", 0, "UnloadFull") //AmmoFull
		TNT1 A 0 A_JumpIfInTargetInventory("BeowulfClip", 1, "UnloadAmmo")
		TNT1 A 0 A_Scream
		TNT1 A 0 A_NoBlocking
		TNT1 A 1
		Stop
		
    UnloadAmmo:
        TNT1 A 0 A_TakeFromTarget("BeowulfClip", 1)
        TNT1 A 0 A_GiveToTarget("Ammo44mag", 1)
        TNT1 A 0 A_JumpIfInTargetInventory("Ammo44mag", 0, "UnloadFull") //AmmoFull
        TNT1 A 0 A_JumpIfInTargetInventory("BeowulfClip", 1, "UnloadAmmo")
        Goto UnloadEnd
	
	UnloadEnd:
		TNT1 A 0
		TNT1 A 0 A_GiveToTarget("BeowulfEmpty", 1)
		TNT1 A 0 A_Scream
		TNT1 A 0 A_NoBlocking
		TNT1 A 1
		Stop
		
	UnloadFull:
		TNT1 A 0
		TNT1 A 0 A_JumpIfInTargetInventory("BeowulfClip", 1, 2)
		TNT1 A 0 A_GiveToTarget("BeowulfEmpty", 1)
		TNT1 A 0 A_Scream
		TNT1 A 0 A_NoBlocking
		TNT1 A 1
		Stop
		
	Stoped:
	    TNT1 A 0
		TNT1 A 1
		stop
	}
}


ACTOR DropWeapon_Beowulf
{
	Radius 8
	Height 8
	Speed 20
	mass 2
	Damage 2
	 +DONTHURTSPECIES
  +THRUSPECIES
  +MTHRUSPECIES
  Species "Marines"
  damagetype "Kick"
        +NoBlockmap
		+DropOff
        +ActivatePCross
        +ActivateImpact
        -NoTeleport
        +DontFall
        +Missile
          DeathSound "none"
	 scale 0.16
  States
  {
  Spawn:
    B004 K 1
		loop
	Death:
		TNT1 A 0 A_SpawnItemEx ("Beowulf_PickupE", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		Stop
	}
}

