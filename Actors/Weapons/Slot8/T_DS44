actor Beowulf : RGUWeapon
{
  Obituary "%o was shot down by %k's Beowulf."
  inventory.pickupmessage "[8] Alexander Arms Beowulf .44 Magnum semi-automatic rifle"
  inventory.pickupsound "misc/w_pkup" 
  weapon.selectionorder 50
  tag "Beowulf"
  weapon.ammotype "BeowulfClip"
  weapon.ammouse 1
  weapon.ammotype2 "Ammo44mag"
  +NOAUTOFIRE
  States
  {
  Spawn:
  B004 K 0
	TNT1 A 0 A_PlaySound("WeaponDropped") 
    TNT1 A 2
	TNT1 A 0 A_SpawnItemEx ("RemoveSlot8", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
	TNT1 A 0 A_SpawnItemEx ("RemoveBeowulf", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
	TNT1 A 0 A_SpawnItemEx ("DropWeapon_Beowulf", 20, 0, 20, 10, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
    stop
  Select:
    TNT1 A 1 A_Raise
    TNT1 AA 0 A_Raise
    loop
  Ready:
   TNT1 A 0 A_TakeInventory("Weight", 0)
	TNT1 A 0 A_GiveInventory("Weight", 3)	//Determines player speed - 3 means slower speed
    TNT1 A 0 ACS_NamedExecuteAlways("CoDPlayerSpeed", 0, 0, 0, 0)
	TNT1 A 0 ACS_NamedExecuteWithResult("CoDWeaponLook", 56, 0, 0)
	TNT1 A 0 ACS_NamedExecuteWithResult("CoDFireSelectIcon", 99, 0, 0)
	TNT1 A 0 ACS_NamedExecuteWithResult("CoDScopeSelectIcon", 99, 0, 0)
	TNT1 A 0 ACS_NamedExecuteWithResult("CoDFireSelect", 12, 0, 0)
	TNT1 A 0 A_TakeInventory("BOSelectScope", 1)
	TNT1 A 0 A_TakeInventory("BOSelectFire", 1)
	TNT1 A 0 A_TakeInventory("BOWeaponlook", 1)
	TNT1 A 0 A_TakeInventory("BOWeaponlooking", 1)
	TNT1 A 0 A_TakeInventory("Reloading", 1)
	TNT1 A 0 A_TakeInventory("Aiming", 1)
	GUNP A 0 //ACS_NamedExecuteWithResult("CoDWeaponIcon", 84, 0, 0)
	TNT1 A 0 A_ZoomFactor(1.0, ZOOM_INSTANT)
    TNT1 A 0 A_SetCrosshair(0)
    TNT1 A 0 A_JumpIfInventory("Perk1_SOH_Pro", 1, "GiveSOHSelect")
    RR4A A 2 A_PlayWeaponSound("weapon/select")
	RR4A BCDEGH 2
    //goto InHand 				-- intentional drop to next state
  InHand: 					//Ready state
    RR4A F 0 A_JumpIfNoAmmo("Reload")
    RR4A F 0 A_JumpIfInventory("Aiming", 1, "Aiming")
    RR4A F 0 A_JumpIfInventory("Reloading", 1, "Reload")
    RR4A F 0 A_JumpIfInventory("ThrowingGrenade", 1, "KnifeAttack")
    RR4A F 0 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
	TNT1 A 0 //A_JumpIfInventory("BOSelectFire", 1, "FireSet")
	TNT1 A 0 //A_JumpIfInventory("BOSelectScope", 1, "ScopeSet")
	TNT1 A 0 A_JumpIfInventory("BOWeaponlook", 1, "Weaponlook")
	TNT1 A 0 A_JumpIfInventory("BONVstart", 1, "NightVisionON") //
	GUNP A 0 //ACS_NamedExecuteWithResult("CoDWeaponIcon", 84, 0, 0)
    RR4A F 1 A_WeaponReady(WRF_ALLOWRELOAD)
	Goto CheckFiremode 
	
  Friendcallup:
    TNT1 A 0 A_ZoomFactor(1.0)
    TNT1 A 0 A_SetCrosshair(0)
    TNT1 A 0 A_GiveInventory("Followcall", 1)
    TNT1 A 0 A_TakeInventory("Aiming", 1)
    TNT1 A 0 A_TakeInventory("Reloading", 1)
    TNT1 A 0 ACS_NamedExecuteAlways("CoDPlayerSpeed", 0, 0, 0, 0)
    TNT1 A 0 A_PlaySound("NEEDCOME")
    FRGE IHGFGHI 2
    TNT1 A 10
    TNT1 A 0 A_TakeInventory("KnifeAttack", 1)
    TNT1 A 0 A_TakeInventory("Followcall", 1)
    TNT1 A 0 A_TakeInventory("BOFriendcall", 1)
    Goto Ready2+2
  Ready2:
    TNT1 A 0 //A_GiveInventory("Weight", 3)	
    TNT1 A 0 ACS_NamedExecuteAlways("CoDPlayerSpeed", 0, 0, 0, 0)
    TNT1 A 0 A_JumpIfInventory("Perk1_SOH_Pro", 1, "GiveSOHSelect")
    TNT1 A 0 A_PlayWeaponSound("weapon/select")
    RR4A ABCDEGH 2 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
    goto InHand

  CheckFiremode:
   ACRA F 0
   ACRA F 0 A_JumpIfInventory("Beowulf_FireSet", 1, 2)
   ACRA F 0 //ACS_NamedExecuteWithResult("CoDFireSelectIcon", 1, 0, 0)//Full
   Goto CheckScopemode
   ACRA F 0 //ACS_NamedExecuteWithResult("CoDFireSelectIcon", 4, 0, 0)//Semi
   Goto CheckScopemode

  CheckScopemode:
   ACRA F 0
   ACRA F 0 //A_JumpIfInventory("AN94_Scope", 1, 2)
   ACRA F 0 //ACS_ExecuteWithResult(995, 1, 0, 0)//x3
   Goto Inhand
   ACRA F 0 //ACS_ExecuteWithResult(995, 2, 0, 0)//Dot
   Goto Inhand

  FireSet:
    TNT1 A 0 
	TNT1 A 0 A_JumpIfInventory("Beowulf_FireSet", 1, "FireSetoff")   
    TNT1 A 0 A_PlaySoundEx("FAL/set", "SoundSlot5")
	//TNT1 A 0 ACS_ExecuteWithResult(894, 4, 0, 0)
	ACRA F 0 //ACS_NamedExecuteWithResult("CoDFireSelectIcon", 4, 0, 0)//Semi
	TNT1 A 0 A_TakeInventory("BOSelectFire", 1)
    TNT1 A 0 A_GiveInventory("Beowulf_FireSet", 1)
    goto InHand
  FireSetoff:
    TNT1 A 0 
    TNT1 A 0 A_PlaySoundEx("FAL/set", "SoundSlot5")
	//TNT1 A 0 ACS_ExecuteWithResult(894, 1, 0, 0)
	ACRA F 0 //ACS_NamedExecuteWithResult("CoDFireSelectIcon", 1, 0, 0)//Full
	TNT1 A 0 A_TakeInventory("BOSelectFire", 1)
    TNT1 A 0 A_TakeInventory("Beowulf_FireSet", 1)
    goto InHand
	
    ScopeSet: //Hipfire
    TNT1 A 0 
	//TNT1 A 0 ACS_ExecuteWithResult(895, 2, 0, 0)
	ACRA F 0 //ACS_NamedExecuteWithResult("CoDScopeSelectIcon", 2, 0, 0)
	TNT1 A 0 A_TakeInventory("BOSelectScope", 1)
    goto InHand
	
	  NightVisionON:
    TNT1 A 0 
	TNT1 A 0 A_TakeInventory("Aiming", 1)
	TNT1 A 0 A_ZoomFactor(1.0, ZOOM_INSTANT)
    TNT1 A 0 A_SetCrosshair(0)
	TNT1 A 0 A_JumpIfInventory("NightVisionActive", 1, "NightVisionOFF")   
	TNT1 A 0 A_TakeInventory("BONVstart", 1)
    TNT1 A 0 A_PlaySoundEx("g36c/reload1", "SoundSlot5")
	BONV ABCDEFGHIJKLM 2
	BONV N 6
	TNT1 A 0 A_PlaySoundEx("BONVactive", "SoundSlot2")
	TNT1 A 0 A_GiveInventory("Infrared_COD", 1)
	TNT1 A 0 A_GiveInventory("NightVisionActive", 1)
	TNT1 A 0 A_TakeInventory("BONVstart", 1)
    goto ready2
  NightVisionOFF:
    TNT1 A 0 
	TNT1 A 0 A_TakeInventory("Infrared_COD", 0)
	TNT1 A 0 A_TakeInventory("PowerLightAmp", 0)
	TNT1 A 0 A_TakeInventory("NightVisionActive", 0)
	TNT1 A 0 A_TakeInventory("BONVstart", 1)
    TNT1 A 0 A_PlaySoundEx("weapon/select", "SoundSlot5")
	BONV NMLKJIHGFEDCBA 2
	TNT1 A 0 A_TakeInventory("BONVstart", 1)
    goto ready2
	
  Weaponlook:
    TNT1 A 0 A_TakeInventory("BOWeaponlook", 1)
	TNT1 A 0 A_TakeInventory("Aiming", 1)
  Weaponlooking: 	
    TNT1 A 0 A_GiveInventory("BOWeaponlooking", 1)
    ACRA F 0 A_JumpIfInventory("Aiming", 1, "WeaponlookEND")
    ACRA F 0 A_JumpIfInventory("Reloading", 1, "WeaponlookEND")
    ACRA F 0 A_JumpIfInventory("ThrowingGrenade", 1, "WeaponlookEND")
    ACRA F 0 A_JumpIfInventory("KnifeAttack", 1, "WeaponlookEND")
	ACRA F 0 A_JumpIfInventory("BOWeaponlook", 1, "WeaponlookEND")
	TNT1 A 0 ACS_NamedExecuteWithResult("CoDWeaponLook", 84, 0, 0)
    RR4A F 1 A_WeaponReady(WRF_ALLOWRELOAD)
    loop 
  WeaponlookEND:
    TNT1 A 0 ACS_NamedExecuteWithResult("CoDWeaponLook", 56, 0, 0)
    TNT1 A 0 A_TakeInventory("BOWeaponlook", 1)
    TNT1 A 0 A_TakeInventory("BOWeaponlooking", 1)
    goto InHand
   
  Aiming:
    RR4C D 0 A_JumpIfInventory("Reloading", 1, "Reload")    
    RR4C D 0 A_JumpIfInventory("ThrowingGrenade", 1, "KnifeAttack")
    RR4C D 0 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
	ACRA F 0 //A_JumpIfInventory("BOSelectFire", 1, "FireSet")
	ACRA F 0 A_GunFlash
	RR4C D 1 A_WeaponReady(WRF_NOBOB|WRF_ALLOWRELOAD)
	TNT1 F 0 
    loop
	
  Deselect:
    TNT1 A 0 A_TakeInventory("Weight", 0)	//Determines player speed - 1 means normal speed
    TNT1 A 0 A_TakeInventory("Aiming", 1)
    TNT1 A 0 ACS_NamedExecuteAlways("CoDPlayerSpeed", 0, 0, 0, 0)
    TNT1 A 0 A_TakeInventory("Reloading", 1)
	TNT1 A 0 ACS_NamedExecuteWithResult("CoDWeaponLook", 56, 0, 0)
	TNT1 A 0 A_TakeInventory("BOWeaponlook", 1)
    TNT1 A 0 A_TakeInventory("BOWeaponlooking", 1)
    TNT1 A 0 A_ZoomFactor(1.0)
    TNT1 A 0 A_SetCrosshair(0)
    TNT1 A 0 A_JumpIfInventory("Perk1_SOH_Pro", 1, "GiveSOHDeselect")
    RR4A HGEDCBA 1
  DeselectFull: 				//Deselect
    TNT1 AA 0 A_Lower
    TNT1 A 1 A_Lower
    loop  
  Fire:
    ACRA F 0 A_JumpIfInventory("BOWeaponlooking", 1, "WeaponlookEND")
    RR4A F 0 A_JumpIfInventory("Aiming", 1, "FireAim")
    RR4A F 0 A_JumpIfNoAmmo("ReloadStart")
    RR4B A 0 A_FireCustomMissile("BeowulfPuff", 0, 0, 10, -3)   //Fire effect and sound
	TNT1 A 0 A_PlaySoundEx("anaconda/fire1", "SoundSlot5")
    TNT1 A 0 A_PlaySoundEx("M24SWS/fire2", "SoundSlot6")
	RR4B A 1 BRIGHT A_RailAttack(130, 0, 1, "none", "none", RGF_SILENT, 8, "ShotgunPuff", 0.1, 0.1, 14400, 0, 1.0, 1.0, "none", 4)
	TNT1 F 0 BRIGHT A_FireBullets(0.0, 0.0, -1, 40, "Bullet762NATO", 0, 9000)
	TNT1 A 0 A_FireCustomMissile("HVRifleCaseSpawn", 5, 0, 8, -9)
    RR4B B 0 ACS_NamedExecuteAlways("CoDRecoil", 0, 200, 200, 8)		//Recoil script - 600 recoil, revert aim
    RR4B B 1 BRIGHT 
	RR4B D 0 A_JumpIfNoAmmo("FireEmpty")
	RR4B CD 1 A_WeaponReady(WRF_ALLOWRELOAD)
    RR4B EF 1 A_WeaponReady(WRF_ALLOWRELOAD)
	RR4A F 4 A_WeaponReady(WRF_ALLOWRELOAD)
    RR4B A 0 A_JumpIfInventory("Ammo44mag", 1, 2)
    RR4B H 0 A_ReFire
    goto InHand
    RR4B A 0 A_JumpIfNoAmmo(2)
    RR4B A 0 A_ReFire
    goto InHand
    RR4B A 0 A_GiveInventory("BeowulfEmpty", 1)		//Go to reload
    goto Reload
  FireEmpty:
    RR4B C 1 
	M40C D 0 A_PlayWeaponSound("weapon/dryfire")
    RR4B A 0 A_JumpIfNoAmmo(2)
    RR4B A 0 A_ReFire
    goto InHand
    RR4B A 0 A_GiveInventory("BeowulfEmpty", 1)		//Go to reload
    goto Reload
  FireAim:
    RR4C D 0 A_JumpIfNoAmmo("ReloadStart")
    RR4D A 0 A_FireCustomMissile("BeowulfPuff", 0, 0, 0, -4)   //Fire effect and sound
	TNT1 A 0 A_PlaySoundEx("anaconda/fire1", "SoundSlot5")
    TNT1 A 0 A_PlaySoundEx("M24SWS/fire2", "SoundSlot6")
    RR4D A 1 BRIGHT A_RailAttack(130, 0, 1, "none", "none", RGF_SILENT, 8, "ShotgunPuff", 0.0, 0.0, 14400, 0, 1.0, 1.0, "none", 4)
	TNT1 F 0 BRIGHT A_FireBullets(0.0, 0.0, -1, 40, "Bullet762NATO", 0, 9000)
	TNT1 A 0 A_FireCustomMissile("HVRifleCaseSpawn", 5, 0, 8, -25)
    RR4D B 0 ACS_NamedExecuteAlways("CoDRecoil", 0, 200, 200, 8)		//Recoil script - 600 recoil, revert aim
    ACRA F 0 A_GunFlash
	RR4D B 1 BRIGHT 
	RR4D D 0 A_JumpIfNoAmmo("FireEmptyAim")
	ACRA F 0 A_GunFlash
	RR4D CD 1 A_WeaponReady(WRF_ALLOWRELOAD)
	ACRA F 0 A_GunFlash
    RR4D EF 1 A_WeaponReady(WRF_ALLOWRELOAD)
	ACRA F 0 A_GunFlash
    RR4C D 2 A_WeaponReady(WRF_ALLOWRELOAD)
	ACRA F 0 A_GunFlash
    RR4C D 2 A_WeaponReady(WRF_ALLOWRELOAD)
	ACRA F 0 A_GunFlash
    RR4D A 0 A_JumpIfInventory("Ammo44mag", 1, 2)
    RR4D D 0 A_ReFire
    goto InHand
    RR4D A 0 A_JumpIfNoAmmo(2)
    RR4D A 0 A_ReFire
    goto InHand
    RR4D A 0 A_GiveInventory("BeowulfEmpty", 1)		//Go to reload
    goto Reload
  FireEmptyAim:
    RR4C D 0 A_PlayWeaponSound("weapon/dryfire")
    RR4D A 0 A_JumpIfNoAmmo(2)
    RR4D A 0 A_ReFire
    goto InHand
    RR4D A 0 A_GiveInventory("BeowulfEmpty", 1)		//Go to reload
    goto Reload
  Hold:
    RR4A F 0 A_JumpIfInventory("Aiming", 1, "HoldAim")
    RR4A F 1 
    RR4A F 0 A_JumpIfInventory("ThrowingGrenade", 1, "KnifeAttack")
    RR4A F 0 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
	RR4A F 1 A_ReFire
    goto Inhand
  HoldAim:
    RR4C D 1 A_GunFlash
    RR4C D 0
	RR4C D 1 A_ReFire
    goto Aiming
	
  AltFire:	//Aim
    ACRA F 0 A_JumpIfInventory("BOWeaponlooking", 1, "WeaponlookEND")
    RR4C A 0 A_JumpIfInventory("Aiming", 1, "AltFire2")
    RR4C A 0 A_ZoomFactor(1.8)
    TNT1 A 0 A_SetCrosshair(41)
    RR4C ABC 1
	RR4C EF 1 A_GunFlash
    RR4C D 0 A_GiveInventory("Aiming", 1)
    RR4C D 0 ACS_NamedExecuteAlways("CoDPlayerSpeed", 0, 0, 0, 0)
    S58A F 0 A_ReFire
    goto Aiming
  AltFire2:
    RR4C D 0 A_ZoomFactor(1.0)
    TNT1 A 0 A_SetCrosshair(0)
	RR4C FECBA 1
    RR4C A 0 A_TakeInventory("Aiming", 1)
	TNT1 A 0 A_TakeInventory("BONVstart", 1)
	TNT1 A 0 A_TakeInventory("BOWeaponlook", 1)
    RR4C A 0 ACS_NamedExecuteAlways("CoDPlayerSpeed", 0, 0, 0, 0)
    S58A F 0 A_ReFire
    goto InHand
	
	
  AltFire_QD:	//Aim with QuickDraw
    RR4C A 0 A_JumpIfInventory("Aiming", 1, "AltFire2_QD")
    RR4C A 0 A_ZoomFactor(1.4)
    TNT1 A 0 A_SetCrosshair(41)
    RR4C ABCD 1
    RR4C D 0 A_GiveInventory("Aiming", 1)
    RR4C D 0 ACS_NamedExecuteAlways("CoDPlayerSpeed", 0, 0, 0, 0)
    RR4A F 0 A_ReFire
    goto Aiming
  AltFire2_QD:
    RR4C D 0 A_ZoomFactor(1.0)
    TNT1 A 0 A_SetCrosshair(0)
    RR4C DCBA 1
    RR4C A 0 A_TakeInventory("Aiming", 1)
    RR4C A 0 ACS_NamedExecuteAlways("CoDPlayerSpeed", 0, 0, 0, 0)
    RR4A F 0 A_ReFire
    goto InHand
	
  AltHold:
    RR4A F 1 A_JumpIfInventory("Aiming", 1, "AltHoldAim")
	TNT1 F 0 
    RR4A F 1 A_ReFire
    goto InHand
  AltHoldAim:
    RR4C D 1 A_GunFlash
	TNT1 F 0 
    RR4C D 1 A_ReFire
    goto Aiming
  ReloadStartAim:
    RR4C D 0 A_ZoomFactor(1.0)
    TNT1 A 0 A_SetCrosshair(0)
	RR4C FECBA 1
    RR4C A 0 A_TakeInventory("Aiming", 1)
    RR4C A 0 ACS_NamedExecuteAlways("CoDPlayerSpeed", 0, 0, 0, 0)
    goto ReloadStart+1
  Reload:
    TNT1 A 0 A_TakeInventory("Reloading", 1)
    TNT1 A 0 A_JumpIfInventory("BeowulfClip", 0, "InHand")
    TNT1 A 0 ACS_NamedExecuteWithResult("CoDWeaponLook", 56, 0, 0)
    TNT1 A 0 A_JumpIfInventory("Ammo44mag", 1, "ReloadStart")
	TNT1 A 0 A_JumpIfInventory("Aiming", 1, "AltFire2")
    goto InHand+3
	
  ReloadStart:
    TNT1 A 0 A_JumpIfInventory("Perk1_SOH", 1, "GiveSOHReload")
    RR4E A 0 A_JumpIfInventory("Aiming", 1, "ReloadStartAim")
	RR4E A 0 A_JumpIfInventory("BeowulfEmpty", 1, "ReloadStartEmpty")
    RR4E E 0 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE)
    RR4E ABCDEF 2 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
	RR4A C 0 A_PlayWeaponSound("ADAR/reload1")
	RR4E I 0 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE)		
	RR4E GHIJKLMNOPQRSTUVWX 2 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
	RR4E YZ 2 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
	RR4F AB 2 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
	RR4E I 0 A_PlayWeaponSound("ADAR/reload2")
    goto ReloadAmmo
  ReloadAmmo:
    TNT1 A 0 A_GiveInventory("BeowulfClip", 1)
    TNT1 A 0 A_TakeInventory("Ammo44mag", 1)
    TNT1 A 0 A_JumpIfInventory("BeowulfClip", 0, "ReloadEnd")
    TNT1 A 0 A_JumpIfInventory("Ammo44mag", 1, "ReloadAmmo")
    goto ReloadEnd
  ReloadEnd:
    TNT1 A 0 A_TakeInventory("Reloading", 1)
    RR4F G 0 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE)
	RR4F CDEFGHIJKLM 2 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
    TNT1 A 0 A_ReFire
    goto InHand
	
  ReloadStartEmpty:
    TNT1 A 0 
    RR4E E 0 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE)
    RR4G ABCDE 2 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
	RR4A C 0 A_PlayWeaponSound("ADAR/reload3")
	RR4E I 0 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE)		
	RR4G FGHIJKLMNOPQRSTU 2 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
	RR4E I 0 A_PlayWeaponSound("ADAR/reload2")
	RR4G VWXYZ 2 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
	RR4H AB 2 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
    goto ReloadAmmoEmpty
  ReloadAmmoEmpty:
    TNT1 A 0 A_GiveInventory("BeowulfClip", 1)
    TNT1 A 0 A_TakeInventory("Ammo44mag", 1)
    TNT1 A 0 A_JumpIfInventory("BeowulfClip", 25, "ReloadEndEmpty")
    TNT1 A 0 A_JumpIfInventory("Ammo44mag", 1, "ReloadAmmoEmpty")
    goto ReloadEndEmpty
  ReloadEndEmpty:
    TNT1 A 0 A_TakeInventory("BeowulfEmpty", 1)
    TNT1 A 0 A_TakeInventory("Reloading", 1)
	RR4H G 0 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE)
	RR4H CD 2 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
	RR4H F 0 A_PlayWeaponSound("Deerstalker/reload4")
	RR4H EFGHIJKLM 2 A_JumpIfInventory("KnifeAttack", 1, "KnifeAttack")
    RR4H G 0 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE)
    TNT1 A 0 A_ReFire
    goto InHand
	
  KnifeAttack:
    TNT1 A 0 A_ZoomFactor(1.0)
    TNT1 A 0 A_SetCrosshair(0)
    TNT1 A 0 A_TakeInventory("Aiming", 1)
    TNT1 A 0 A_TakeInventory("Reloading", 1)
    TNT1 A 0 ACS_NamedExecuteAlways("CoDPlayerSpeed", 0, 0, 0, 0)
    TNT1 A 0 A_JumpIfInventory("ThrowingGrenade", 1, "FragGrenade")
    TNT1 A 0 ACS_NamedExecuteAlways("CoDRecoil", 0, 900, 900, 1)		//Recoil script
	TNT1 A 0 A_JumpIfInventory("Katana", 1, "KatanaAttack")
	TNT1 A 0 A_JumpIfInventory("Katana_Samurai", 1, "KatanaAttack")
    KNI9 A 1 A_PlaySoundEx("melee/hit", "SoundSlot5")
    KNI9 BCL 1
    KNI9 D 1 A_FireBullets(10.0, 0.0, 6, 7, "MachineGunBulletPuff", 0, 80)
    KNI9 EFGHIJ 1
    TNT1 A 5
    TNT1 A 0 A_TakeInventory("KnifeAttack", 1)
    goto Ready2+2
  KatanaAttack:
	KTNB FG 1
	KTNB H 1 A_Saw("weapons/knifeswing3", "weapons/knifehits", 40, "katanaPuff", SF_NOUSEAMMO, 80, 32)
    KTNB IJ 1 
    TNT1 A 5
	TNT1 A 0 A_TakeInventory("KnifeAttack", 1)
    goto Ready2+2
  FragGrenade:
    TNT1 A 0 A_JumpIfInventory("ThrowingStunGrenade", 1, "StunGrenade")
    TNT1 A 0 A_JumpIfInventory("ThrowingKnife", 1, "ThrowingKnife")
    FRGA ABCDEFGHI 1
    FRGA J 0 A_PlaySoundEx("grenade/pinpull", "SoundSlot5")
	FRGA JKLMOPQ 2
    FRGA ST 2
    FRGA H 0 A_PlaySoundEx("grenade/throw", "SoundSlot5")
    FRGA I 0 A_TakeInventory("FragGrenadeAmmo", 1)
    FRGA I 0 A_TakeInventory("ThrowingGrenade", 1)
    FRGA I 0 A_TakeInventory("KnifeAttack", 1)
    FRGA I 0 A_FireCustomMissile("FragGrenade", 0, 0, 0, 16)
	FRGA U 2
	FRGA VWX 2
    goto Ready2+1		//Reselect weapon
  StunGrenade:
    FRGA ABCDEFGHI 1
    FRGA F 0 A_PlaySoundEx("flash/pinpull", "SoundSlot5")
    FRGA JKLMOPQ 1
    FRGA ST 1
    FRGB H 0 A_PlaySoundEx("flash/throw", "SoundSlot5")
    FRGB I 0 A_TakeInventory("ThrowingGrenade", 1)
    FRGB I 0 A_TakeInventory("KnifeAttack", 1)
    FRGB I 0 A_TakeInventory("ThrowingStunGrenade", 1)
    FRGB C 0 A_FireCustomMissile("StunGrenadeThrown", 0, 0, 0, 16)
    FRGA U 2
	FRGA VWX 2
    goto Ready2+1		//Reselect weapon
  SemtexGrenade:
    FRGA ABCDE 3
    FRGA F 3 A_PlaySoundEx("grenade/pinpull", "SoundSlot5")
    TNT1 A 15 
    FRGA GH 3
    FRGA H 0 A_PlaySoundEx("grenade/throw", "SoundSlot5")
    FRGA I 0 A_TakeInventory("ThrowingGrenade", 1)
    FRGA I 0 A_TakeInventory("KnifeAttack", 1)
    FRGB I 0 A_TakeInventory("ThrowingSemtex", 1)
    FRGA I 3 A_FireCustomMissile("SemtexThrown", 0, 0, 0, 16)
    goto Ready+1		//Reselect weapon
  ThrowingKnife:
    FRGC AB 3
    FRGC C 3 A_PlaySoundEx("melee/swing", "SoundSlot5") 
    FRGC D 3 A_FireCustomMissile("ThrowingKnifeThrown", 0, 0, 0, 8)
    FRGC EFG 3
    TNT1 A 15 
    FRGC I 0 A_TakeInventory("ThrowingGrenade", 1)
    FRGC I 0 A_TakeInventory("KnifeAttack", 1)
    FRGC I 0 A_TakeInventory("ThrowingKnife", 1)
    goto Ready+1		//Reselect weapon
  Claymore:
    FRGD A 2 A_PlaySoundEx("claymore/click", "SoundSlot5") 
    FRGD BCDEFGHIJKLM 2
    TNT1 A 3 A_GiveInventory("ClaymoreUse", 1)
    TNT1 A 15 
    FRGC I 0 A_TakeInventory("ThrowingGrenade", 1)
    FRGC I 0 A_TakeInventory("KnifeAttack", 1)
    FRGC I 0 A_TakeInventory("ThrowingClaymore", 1)
    goto Ready+1		//Reselect weapon
  Betty:
    FRGE G 3 A_PlaySoundEx("grenade/throw", "SoundSlot5") 
    FRGE B 3 A_GiveInventory("BettyUse", 1)
    FRGE CDEF 3
    TNT1 A 15 
    FRGC I 0 A_TakeInventory("ThrowingGrenade", 1)
    FRGC I 0 A_TakeInventory("KnifeAttack", 1)
    FRGC I 0 A_TakeInventory("ThrowingBetty", 1)
    goto Ready+1		//Reselect weapon
  Flash:
    TNT1 A 0 
    RR4C X 3 BRIGHT //A_Light2
    stop
  GiveSOHSelect:
    goto Ready+3
  GiveSOHDeselect:
    goto Deselect+6
  GiveSOHReload:
    goto ReloadStart+1
  GiveQDKnife:
    goto KnifeAttack+5
  }
}