		TNT1 A -1 A_SpawnItemEx("Mosin_Pickup_drop", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
		Stop
	SVT40:
		TNT1 A 0 A_SpawnItemEx("SVD_Ammo_E2", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION) 
		TNT1 A -1 A_SpawnItemEx("SVT40_Pickup_drop", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
		Stop
	Win94:
