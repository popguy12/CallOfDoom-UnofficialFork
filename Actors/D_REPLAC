

ACTOR NaziWeaponSpawner1 : CoDSpawner Replaces MP40
{
	States
	{
	Spawn:
		TNT1 A 0
		TNT1 A 0 A_Jump(128, "MP40", "S1100", "M712", "M1917", "Type54") //MP40
		TNT1 A 0 A_Jump(64, "PPSH", "Mosin", "SVT40",  "M30LD", "Win94") //WW2 Weapon
		TNT1 A 0 A_Jump(8, "AVS36", "WA1921", "M1928") //Rare
	MP40:
		TNT1 A 0 A_SpawnItemEx("SMGAmmo1_E2", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
		TNT1 A -1 A_SpawnItemEx("MP40_Pickup_drop", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
		Stop
	S1100:
		TNT1 A 0 A_SpawnItemEx("TT33_Ammo1_E2", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
		TNT1 A -1 A_SpawnItemEx("S1100_Pickup_drop", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
		Stop
	M712:
		TNT1 A 0 A_SpawnItemEx("TT33_Ammo1_E", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
		TNT1 A -1 A_SpawnItemEx("M712_Pickup_drop", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
		Stop
	M30LD:
		TNT1 A 0 A_SpawnItemEx("SGHAmmo_M30", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION) 
		TNT1 A -1 A_SpawnItemEx("M30LD_Pickup", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
		Stop
	PPSH:
		TNT1 A 0 A_SpawnItemEx("TT33_Ammo1_E2", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
		TNT1 A -1 A_SpawnItemEx("PPSH41_Pickup", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
		Stop
	M1917:
