		TNT1 A 0 A_SpawnItemEx ("Marine_Woods_A", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
        Stop
	
	DontSpawn:
	    RAN4 A 1
		Goto Spawn	
    }
}

