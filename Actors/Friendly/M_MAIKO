    damagefactor "Blood", 0.0    damagefactor "GreenBlood", 0.0    damagefactor "BlueBlood", 0.0    damagefactor "Taunt", 0.0    damagefactor "KillMe", 0.0
    damagefactor "Blood", 0.0 damagefactor "BlueBlood", 0.0 damagefactor "GreenBlood", 0.0
    damagefactor "Shrapnel", 0.3
	damagefactor "SpawnMarine", 0.0
	damagefactor "HelperMarineFatallity", 0.0
	
	States
	{
	Active:
		TNT1 A 0 A_ChangeFlag("NOBLOCKMAP", 0)
		TNT1 A 0 A_PlaySound("Lucy/wt", 2)
		TNT1 A 0 A_CustomMissile ("OrderTitle2", 50, 0, 0, 2, 90)
		TNT1 A 0 A_GiveInventory("PassClaymore", 1)
		TNT1 A 0 A_SpawnItemEx ("Marine_Maiko_G", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		Stop
		
	Spawn:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0 A_GiveInventory("PassClaymore", 1)
		TNT1 A 0
		TNT1 A 0 A_GiveInventory("TargetIsAMarine")
		MIKO S 20
		//MIKO A 1 A_Look
		Goto See
	
	ForgetTarget:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0 A_ClearTarget
		Goto See
	
	See:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0 A_ChangeFlag("NODROPOFF", 0)	
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//

		TNT1 A 0 A_ChangeFlag("MISSILEMORE", 1)
		TNT1 A 0 A_ChangeFlag("MISSILEEVENMORE", 1)
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		MIKO AA 1 A_Chase
		TNT1 A 0 A_ChangeFlag("FASTER", 0)
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		MIKO BB 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		MIKO BB 1 A_Chase
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		MIKO CC 1 A_Chase
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 1)
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		MIKO CC 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		MIKO DD 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		MIKO DD 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_CheckSight("ForgetTarget")
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_CheckSight("FollowPLayer")
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		
		MIKO AA 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfCloser(300, "Waits")
		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
		
		MIKO BB 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "FollowPlayer")
		TNT1 A 0 A_JumpIfCloser(300, "Waits")
		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
		
		MIKO CC 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "FollowPlayer")
		TNT1 A 0 A_JumpIfCloser(300, "Waits")
		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
		
		MIKO DD 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "FollowPlayer")
		TNT1 A 0 A_JumpIfCloser(300, "Waits")
		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")

		TNT1 A 0 A_JumpIfCloser(300, "Waits")
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "FollowPlayer")
		Goto See
	BecomeEnemy23:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		MIKO A 1
		TNT1 A 0 A_ClearTarget
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		Goto FollowPlayer
		
	FollowPlayer:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 1)
	    TNT1 A 0 A_ChangeFlag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("FASTER", 1)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_GiveToTarget("NumberOfMarineMaiko", 1)
	    TNT1 A 0 A_JumpIfCloser(200, "Waits")
		TNT1 A 0 A_ChangeFlag("MISSILEMORE", 0)
		TNT1 A 0 A_ChangeFlag("MISSILEEVENMORE", 0)
		MIKO A 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_Recoil(-1)
		MIKO AA 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		MIKO BB 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		MIKO BB 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		MIKO CC 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		MIKO CC 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		MIKO DD 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		MIKO DD 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		
		MIKO AA 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		MIKO BB 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		MIKO BB 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		MIKO CC 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
		TNT1 A 0 A_Recoil(-1)
		MIKO CC 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		MIKO DD 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		MIKO DD 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 1)
		TNT1 A 0 A_JumpIfTargetInLOS("See")
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 0)

		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
		Loop
	
	CheckRangeToWait:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0
		TNT1 A 0 A_JumpIfCloser(200, "Waits")
		Goto FollowPLayer
	
	CheckIfPlayerSee:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0
		MIKO AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA 0 A_FastChase
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		////TNT1 A 0 Thing_Hate(94, 0, 0)
		TNT1 A 0 A_JumpIfCloser(800, 2)
		Goto Pathfind
		TNT1 AAA 0
		Goto FollowPlayer

	Pathfind:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_JumpIFInTargetInventory("IsPLayer", 1, 1)
		Goto FollowPLayer
		//TNT1 A 1 A_Chase("", "")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFlag("NOBLOCKMAP", 1)
		TNT1 A 0 A_ChangeFlag("NOCLIP", 1)
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
		TNT1 A 0 A_Noblocking
		TNT1 A 0 A_SpawnItem("TeleportFog")
		TNT1 A 0 A_SpawnItemEx ("Marine1Pathfinder_Maiko", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		Stop

	Waits:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		MIKO R 1
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, 2)
		TNT1 A 0 A_ClearTarget
		Goto See
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("MISSILEMORE", 1)
		TNT1 A 0 A_ChangeFlag("MISSILEEVENMORE", 1)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
		TNT1 A 0 A_GiveToTarget("NumberOfMarineMaiko", 1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "BecomeEnemy23")
		TNT1 A 0 A_Jump(32, "Waits2")
		TNT1 A 0 A_Jump(32, "Waits3")
		
		MIKO R 8 A_Stop
		MIKO R 8 A_Stop
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_Stop
		TNT1 A 0 A_CheckSight("FollowPlayer")
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 1)
		TNT1 A 0 A_ClearTarget
		
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		MIKO E 0 A_Look
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		MIKO R 1 A_LookEx(0, 0, 0, 0, 0, "FollowPlayer")
		TNT1 A 0 A_CheckSIght("FollowPlayer")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_JumpIfCloser(300, "Waits")
		Goto FollowPlayer
	
	Waits2:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0 A_SetAngle(random(90, -90) + angle)
		MIKO R 8 A_Stop
		MIKO R 8 A_Stop
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_CheckSight("FollowPlayer")
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 1)
		TNT1 A 0 A_ClearTarget
		
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		MIKO E 0 A_Look
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		MIKO R 1 A_LookEx(0, 0, 0, 0, 0, "FollowPlayer")
		TNT1 A 0 A_CheckSIght("FollowPlayer")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_JumpIfCloser(400, "Waits")
		Goto FollowPLayer
	
	Waits3:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 AAA 0
		TNT1 A 0 A_SetAngle(random(90, -90) + angle)
		MIKO R 8 A_Stop
		TNT1 A 0 A_PlaySound("Lucy/wait", 2)
		MIKO R 8 A_Stop
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_CheckSight("FollowPlayer")
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 1)
		TNT1 A 0 A_ClearTarget
		
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		MIKO E 0 A_Look
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		MIKO R 1 A_LookEx(0, 0, 0, 0, 0, "FollowPlayer")
		TNT1 A 0 A_CheckSIght("FollowPlayer")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_JumpIfCloser(400, "Waits")
		Goto FollowPLayer
	
	GiveAmmo:
	    TNT1 A 0 A_TakefromTarget("FragGrenadeAmmo", 1)	
		MIKO E 30 A_FaceTarget
		TNT1 E 0 A_CustomMissile ("FriendGiveAmmo_45ACP", 32, 0, 0, 0, 35) 
		MIKO E 30
		Goto Waits
		
	GiveStim:
	    TNT1 A 0 A_TakefromTarget("StunGrenadeInventory", 1)	
		TNT1 A 0 A_TakefromTarget("Stimcall", 1)
		MIKO E 30 A_FaceTarget
		TNT1 E 0 A_CustomMissile ("FriendGiveStimpack", 32, 0, 0, 0, 35) 
		MIKO E 30
		Goto Waits
	
	Missile:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
        //PLAY E 1
		TNT1 A 0 A_ChangeFlag("NODROPOFF", 1)
		TNT1 A 0 A_SpawnItem("KillMeSmall")
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "CheckRangeToWait")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_ChangeFlag("SOLID", 1)
		TNT1 E 0 A_Jump(12, "Talk")
		
	  MIKO E 2 A_FaceTarget
	  TNT1 A 0 A_JumpIfInTargetInventory("PassClaymore", 1, "See")
	  TNT1 A 0 //A_Recoil(2)
	  TNT1 A 0 A_AlertMonsters
      MIKO E 1 Bright A_CustomBulletAttack (3, 3, 6, 8, "friendbullet") 
	  TNT1 A 0 //A_SpawnItem ("BigShotCaseSpawn", 0, 40, 0)
	  TNT1 E 0 A_custommissile("bia_gunflash", 40, 0) 
      MIKO E 6 
	  TNT1 A 0 A_JumpIfCloser(200, "Shotchange2")
	  TNT1 A 0 //A_Recoil(2)
      Goto See
	  
	  Shotchange2:
	  TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
	  //PLAY E 1
	  TNT1 A 0 A_ChangeFlag("NODROPOFF", 1)
	  TNT1 A 0 A_SpawnItem("KillMeSmall")
	  TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
      TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "CheckRangeToWait")
	  TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
	  TNT1 A 0 A_ChangeFlag("SOLID", 1)
	  MIKO E 2 A_FaceTarget
	  TNT1 A 0 A_JumpIfInTargetInventory("PassClaymore", 1, "See")
	  TNT1 A 0 A_Recoil(4)
	  TNT1 A 0 A_AlertMonsters
      MIKO E 1 Bright A_CustomBulletAttack (3, 3, 6, 8, "friendbullet") 
	  TNT1 A 0 //A_SpawnItem ("BigShotCaseSpawn", 0, 40, 0)
	  TNT1 E 0 A_custommissile("bia_gunflash", 40, 0) 
      MIKO E 6 A_Recoil(2)
      MIKO E 1 //A_CPosRefire 
	  TNT1 A 0 ThrustThing (random(0, 255), random(2, 8))
      MIKO E 3 ThrustThingZ(0, 3, 0, 0)
	  TNT1 A 0 A_Recoil(4)
      Goto See

	  Talk:
	     TNT1 A 0 A_PlaySound("Lucy/shot", 2)
	     Goto see

	AttackMelee:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0
		MIKO A 4 A_FaceTarget
		TNT1 A 0 A_Recoil(-4)
		MIKO A 1 A_FaceTarget
		MIKO E 4 A_CustomMissile("MarinePunch", 40, 0, 0, 0)
		MIKO A 2
		Goto See

	Pain.Kick:
		MIKO A 2 A_FaceTarget
		TNT1 A 0 A_Recoil(5)
        Goto FollowPlayer
	
	Pain:
	    MIKO G 3
		TNT1 A 0 A_ChangeFlag("NODROPOFF", 0)
		MIKO G 3 A_Pain
		Goto FollowPlayer

	Death:
        MIKO H 4
		TNT1 A 0 //A_CustomMissile ("MuchBlood", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 0)
        MIKO I 4 A_Scream
		TNT1 A 0 //A_SpawnItemEx("RemoveMaiko", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
        MIKO J 4 A_Fall
        MIKO K 4
		TNT1 A 0 //A_SpawnItem ("MediumBloodSpot")
		TNT1 A 0 A_SpawnItemEx ("marine_Maiko_dead", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
        Stop
	}
}

