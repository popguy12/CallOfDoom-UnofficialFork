		TNT1 A 1
		Stop
	}
}

ACTOR Marine_Maiko_G: Marine_Maiko_A
{
	FastSpeed 0
	MaxStepHeight 0
	+FRIENDLY
	+MISSILEMORE
	+NODROPOFF
	+MISSILEEVENMORE
	+THRUACTORS
	Speed 0
	States
	{
	Active:
		TNT1 A 0 A_ChangeFlag("NOBLOCKMAP", 0)
		TNT1 A 0 A_PlaySound("Lucy/ac", 2)
		TNT1 A 0 A_CustomMissile ("OrderTitle1", 50, 0, 0, 2, 90)
		TNT1 A 0 A_GiveInventory("PassClaymore", 1)
		TNT1 A 0 A_SpawnItemEx ("Marine_Maiko_A", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		Stop
		
	Spawn:
		TNT1 A 0 A_GiveInventory("PassClaymore", 1)
		TNT1 A 0
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 1)
		TNT1 A 0 A_GiveInventory("TargetIsAMarine")
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		MIKO T 20
		MIKO T 1 //A_Look
		Goto See
		
	See:	
		//TNT1 A 0 A_SetAngle(random(90, -90) + angle)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
		MIKO OOOOOOOOOOOOOO 1 A_Chase
		TNT1 E 0 A_Jump(32, "See2")
		TNT1 E 0 A_Jump(16, "See3")
		TNT1 E 0 A_Jump(16, "See4")
		TNT1 E 0 A_Jump(3, "Ha")
		TNT1 A 0 A_ClearTarget
		Loop
		
     See2:
	     MIKO PQP 15 A_Chase
	     Goto see
		
     See3:
	     MIKO QQQ 10 A_Chase
	     Goto see
		 
	 See4:
	     MIKO PPP 10 A_Chase
	     Goto see
		 
	 Ha:
	     TNT1 A 0 A_PlaySound("IRS/ha", 2)
	     Goto see

	Missile:
        //PLAY E 1
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 1)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_ChangeFlag("SOLID", 1)
		TNT1 A 0 A_ChangeFlag("NODROPOFF", 1)
		TNT1 A 0 A_SpawnItem("KillMeSmall")
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "See")
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "See")
		TNT1 E 0 //A_Jump(32, "Talk")
		
      MIKO F 1 A_FaceTarget
	  TNT1 A 0 A_JumpIfInTargetInventory("PassClaymore", 1, "See")
	  TNT1 A 0 A_AlertMonsters
      MIKO F 1 Bright A_CustomBulletAttack (3, 3, 6, 9, "friendbullet") 
	  TNT1 A 0 //A_SpawnItem ("BigShotCaseSpawn", 0, 30, 0)
	  TNT1 E 0 A_custommissile("bia_gunflash", 16, 0) 
      MIKO F 7 
      Goto See
	  
	  Talk:
	     TNT1 A 0 A_PlaySound("Lucy/shot", 2)
	     Goto see
	
	Pain.Kick:
		MIKO A 2 A_FaceTarget
		TNT1 A 0 A_Recoil(5)
        Goto See
			
		
	Pain:
	    MIKO G 3
		MIKO G 3 A_Pain
		TNT1 A 0 A_Recoil(2)
	    TNT1 A 0 ThrustThingZ(0, 3, 0, 0)
		Goto See
	}
