ACTOR Marine_Maiko
{
    Radius 10
    Height 52
	Scale 1.0
	MONSTER
	-SHOOTABLE
	-COUNTKILL
	+FRIENDLY
	//translation "112:127=103:111"
	States
    {
    Spawn:
	    TNT1 A 0 A_GiveInventory("PassClaymore", 1)
		MIKO S 10 
		TNT1 A 0 A_PlaySound("Lucy/shot2", 2)
		MIKO S 10
		MIKO TUS 15 
		//TNT1 A 0 A_Look
	See:	
		//TNT1 A 0 A_GiveToTarget("NumberOfAllies", 1)
		TNT1 A 0 A_SpawnItemEx ("Marine_Maiko_A", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
        Stop
	
	DontSpawn:
	    MIKO A 1
		Goto Spawn	
    }
}

