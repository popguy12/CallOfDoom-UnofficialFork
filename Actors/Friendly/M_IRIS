		TNT1 A 1
		TNT1 A 0 A_TakeFromTarget("NumberOfGirlsIris", 1)
		TNT1 A 0 A_Scream
		TNT1 A 0 A_NoBlocking
		TNT1 A 1
		Stop
	}
}

