	-COUNTKILL
	+FRIENDLY
	States
    {
    Spawn:
		TNT1 A 0 A_GiveInventory("PassClaymore", 1)
		IRIS X 10 
		TNT1 A 0 A_PlaySound("IRS/ac", 2)
		IRIS X 30 
		//TNT1 A 0 A_Look
	See:	
		TNT1 A 0 A_GiveToTarget("NumberOfGirlsIris", 1)
		TNT1 A 0 A_GiveToTarget("GivingAmmo", 1)
		TNT1 A 0 A_SpawnItemEx ("Marine_Iris_A", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
        Stop
	
	DontSpawn:
	    IRIS A 1
		Goto Spawn	
    }
}

