		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 1)
		TNT1 A 0 A_GiveInventory("TargetIsAMarine")
		IRIS O 20
		TNT1 A 0 //A_SpawnItemEx("RemoveIris", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		IRIS E 1 A_Look
		Goto See
		
	See:	
		//TNT1 A 0 A_SetAngle(random(90, -90) + angle)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
		IRIS EEEEEEEEEEEEEEEEE 1 A_Chase
		TNT1 E 0 A_Jump(3, "Ha")
		TNT1 A 0 A_ClearTarget
		Loop

	Ha:
	     TNT1 A 0 A_PlaySound("IRS/ha", 2)
	     Goto see
		
	Missile:
        //PLAY E 1
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 1)
		TNT1 A 0 A_SpawnItem("KillMeSmall")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_ChangeFlag("SOLID", 1)
		TNT1 A 0 A_ChangeFlag("NODROPOFF", 1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "See")
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "See")
		
       IRIS E 1 A_FaceTarget
	  TNT1 E 0 A_custommissile("bia_gunflash", 35, 0) 
	  TNT1 E 0 A_PlaySound("IRS/snipe")
      IRIS F 1 Bright A_VileAttack("snipe/shot", 120, 0, 3, 0, "friendbullet")
	  TNT1 A 0 A_SpawnItem ("HVRifleCaseSpawn", 0, 30, 0)
	  IRIS E 20 
      Goto See
	  
	  Talk:
	     TNT1 A 0 A_PlaySound("IRS/shot", 2)
	     Goto see
		 
	Pain.Kick:
		IRIS A 2 A_FaceTarget
		TNT1 A 0 A_Recoil(5)
        Goto See

	Pain:
	    IRIS G 3
		TNT1 A 0 A_Recoil(5)
		TNT1 G 0 ThrustThing (random(0, 255), random(2, 8))
		IRIS G 3 A_Pain
		Goto See
	}
