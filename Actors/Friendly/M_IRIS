//IRIS
actor FriendProjectileR_Iris
{
	Radius 4
	Height 4
	Speed 4
	Damage 0
                   Scale 0.5
        +NoBlockmap
        +ActivatePCross
        +ActivateImpact
        -NoTeleport
		+CANBOUNCEWATER
        +DropOff
        +DontFall
        +Missile
         +DOOMBOUNCE
          +BOUNCEONACTORS
		  +DONTSPLASH
          DeathSound "misc/teleport"
	States
	{
	Spawn:
		TNT1 A 1 Bright
		Loop
	Death:
		TNT1 A 0 A_SpawnItemEx ("Marine_Iris_A", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		goto death2
	Death2:
		TNT1 A 0 A_SpawnItem("TeleportFog_COD")
		Stop
	}
}

ACTOR Marine1Pathfinder_Iris
{
	Radius 32
