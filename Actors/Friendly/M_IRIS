		TNT1 A 0 A_ChangeFlag("MISSILEEVENMORE", 1)
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		IRIS AA 1 A_Chase
		TNT1 A 0 A_ChangeFlag("FASTER", 0)
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		IRIS BB 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		IRIS BB 1 A_Chase
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		IRIS CC 1 A_Chase
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 1)
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		IRIS CC 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		IRIS DD 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		IRIS DD 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_CheckSight("ForgetTarget")
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_CheckSight("FollowPLayer")
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		
		IRIS AA 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfCloser(200, "Waits")
		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
		
		IRIS BB 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "FollowPlayer")
		TNT1 A 0 A_JumpIfCloser(200, "Waits")
		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
		
		IRIS CC 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "FollowPlayer")
		TNT1 A 0 A_JumpIfCloser(200, "Waits")
		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
		
		IRIS DD 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "FollowPlayer")
		TNT1 A 0 A_JumpIfCloser(200, "Waits")
		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")

		TNT1 A 0 A_JumpIfCloser(200, "Waits")
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "FollowPlayer")
		Goto See
	BecomeEnemy23:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		IRIS A 1
		TNT1 A 0 A_ClearTarget
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		Goto FollowPlayer
		
	FollowPlayer:
	TNT1 A 0 A_GiveToTarget("NumberOfGirlsIris", 1)
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 1)
	    TNT1 A 0 A_ChangeFlag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("FASTER", 1)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
	    TNT1 A 0 A_JumpIfCloser(150, "Waits")
		TNT1 A 0 A_ChangeFlag("MISSILEMORE", 0)
		TNT1 A 0 A_ChangeFlag("MISSILEEVENMORE", 0)
		IRIS A 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_Recoil(-1)
		IRIS AA 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		IRIS BB 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		IRIS BB 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		IRIS CC 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		IRIS CC 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		IRIS DD 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		IRIS DD 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		
		IRIS AA 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		IRIS BB 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		IRIS BB 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		IRIS CC 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
		TNT1 A 0 A_Recoil(-1)
		IRIS CC 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		IRIS DD 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		IRIS DD 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 1)
		TNT1 A 0 A_JumpIfTargetInLOS("See")
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 0)

		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
		Loop
	
	CheckRangeToWait:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0
		TNT1 A 0 A_JumpIfCloser(150, "Waits")
		Goto FollowPLayer
	
	CheckIfPlayerSee:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0
		IRIS AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA 0 A_FastChase
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		////TNT1 A 0 Thing_Hate(94, 0, 0)
		TNT1 A 0 A_JumpIfCloser(800, 2)
		Goto Pathfind
		TNT1 AAA 0
		Goto FollowPlayer

	Pathfind:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_JumpIFInTargetInventory("IsPLayer", 1, 1)
		Goto FollowPLayer
		//TNT1 A 1 A_Chase("", "")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFlag("NOBLOCKMAP", 1)
		TNT1 A 0 A_ChangeFlag("NOCLIP", 1)
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
		TNT1 A 0 A_Noblocking
		TNT1 A 0 A_SpawnItem("TeleportFog")
		TNT1 A 0 A_SpawnItemEx ("Marine1Pathfinder_Iris", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		Stop

	Waits:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		IRIS O 1
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, 2)
		TNT1 A 0 A_ClearTarget
		Goto See
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("MISSILEMORE", 1)
		TNT1 A 0 A_ChangeFlag("MISSILEEVENMORE", 1)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "BecomeEnemy23")
		TNT1 A 0 A_Jump(32, "Waits2")
		TNT1 A 0 A_Jump(32, "Waits3")
		
		IRIS O 8 A_Stop
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_GiveToTarget("NumberOfGirlsIris", 1)
		IRIS O 8 A_Stop
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_GiveToTarget("NumberOfGirlsIris", 1)
		TNT1 A 0 A_Stop
		TNT1 A 0 A_CheckSight("FollowPlayer")
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 1)
		TNT1 A 0 A_ClearTarget
		
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		IRIS O 0 A_Look
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		IRIS O 1 A_LookEx(0, 0, 0, 0, 0, "FollowPlayer")
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_GiveToTarget("NumberOfGirlsIris", 1)
		TNT1 A 0 A_CheckSIght("FollowPlayer")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_JumpIfCloser(200, "Waits")
		Goto FollowPlayer
	
	Waits2:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0 A_SetAngle(random(90, -90) + angle)
		IRIS O 8 A_Stop
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_GiveToTarget("NumberOfGirlsIris", 1)
		IRIS O 8 A_Stop
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_GiveToTarget("NumberOfGirlsIris", 1)
		TNT1 A 0 A_CheckSight("FollowPlayer")
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 1)
		TNT1 A 0 A_ClearTarget
		
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		IRIS O 0 A_Look
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		IRIS O 1 A_LookEx(0, 0, 0, 0, 0, "FollowPlayer")
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_GiveToTarget("NumberOfGirlsIris", 1)
		TNT1 A 0 A_CheckSIght("FollowPlayer")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_JumpIfCloser(300, "Waits")
		Goto FollowPLayer
	
	Waits3:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 AAA 0
		TNT1 A 0 A_SetAngle(random(90, -90) + angle)
		IRIS O 8 A_Stop
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_GiveToTarget("NumberOfGirlsIris", 1)
		TNT1 A 0 //A_PlaySound("IRS/wait", 2)
		IRIS O 8 A_Stop
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_GiveToTarget("NumberOfGirlsIris", 1)
		TNT1 A 0 A_CheckSight("FollowPlayer")
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 1)
		TNT1 A 0 A_ClearTarget
		
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		IRIS O 0 A_Look
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		IRIS O 1 A_LookEx(0, 0, 0, 0, 0, "FollowPlayer")
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_GiveToTarget("NumberOfGirlsIris", 1)
		TNT1 A 0 A_CheckSIght("FollowPlayer")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_JumpIfCloser(300, "Waits")
		Goto FollowPLayer
	
	GiveAmmo:
	    TNT1 A 0 A_TakefromTarget("FragGrenadeAmmo", 1)	
		IRIS X 30 A_FaceTarget
		TNT1 E 0 A_CustomMissile ("FriendGiveAmmo_338", 32, 0, 0, 0, 35) 
		IRIS X 30
		Goto Waits
		
	GiveStim:
	    TNT1 A 0 A_TakefromTarget("StunGrenadeInventory", 1)	
		TNT1 A 0 A_TakefromTarget("Stimcall", 1)
		IRIS X 30 A_FaceTarget
		TNT1 E 0 A_CustomMissile ("FriendGiveMedikit", 32, 0, 0, 0, 35) 
		IRIS X 30
		Goto Waits
	
	Missile:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
        //PLAY E 1
		TNT1 A 0 A_ChangeFlag("NODROPOFF", 1)
		TNT1 A 0 A_SpawnItem("KillMeSmall")
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "CheckRangeToWait")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_ChangeFlag("SOLID", 1)
		 TNT1 E 0 A_Jump(8, "Talk")
		
	  IRIS E 10 A_FaceTarget
	  TNT1 A 0 A_JumpIfInTargetInventory("PassClaymore", 1, "See")
	  TNT1 E 0 A_Jump(32, "Niceshot")
	  TNT1 E 0 A_custommissile("bia_gunflash", 35, 0) 
	  TNT1 F 0 A_PlaySound("IRS/Snipe") //
      IRIS F 1 Bright A_CustomBulletAttack(1, 1, 1, 30, "friendbullet") 
	  TNT1 A 0 ThrustThing (random(0, 255), random(2, 8))
	  TNT1 A 0 A_Recoil(3)
	  IRIS E 5
	  TNT1 A 0 A_SpawnItem("HVRifleCaseSpawn", 0, 20, 0)
	  TNT1 A 0 A_JumpIfCloser(200, "Shotchange")
      Goto See
	  
	  Shotchange:
	    TNT1 A 0 A_ChangeFlag("NODROPOFF", 1)
		TNT1 A 0 A_SpawnItem("KillMeSmall")
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "CheckRangeToWait")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_ChangeFlag("SOLID", 1)
		
	  TNT1 A 0 A_Recoil(1)
	  IRIS P 2 A_FaceTarget
	  TNT1 A 0 A_JumpIfInTargetInventory("PassClaymore", 1, "See")
	  TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "CheckRangeToWait")
	  TNT1 E 0 A_custommissile("bia_gunflash", 35, 0) 
      IRIS P 1 Bright A_CustomBulletAttack(2, 2, 1, 5, "friendbullet") 
	  TNT1 F 0 A_PlaySound("FSMG1/shot") 
	  TNT1 A 0 A_Recoil(1)
	  TNT1 A 0 A_SpawnItem("HGCaseSpawn", 0, 20, 0)
	  
	  IRIS P 2 A_FaceTarget
	  TNT1 A 0 A_JumpIfInTargetInventory("PassClaymore", 1, "See")
	  TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "CheckRangeToWait")
	  TNT1 E 0 A_custommissile("bia_gunflash", 35, 0) 
      IRIS P 1 Bright A_CustomBulletAttack(2, 2, 1, 5, "friendbullet") 
	  TNT1 F 0 A_PlaySound("FSMG1/shot") 
	  TNT1 A 0 A_Recoil(1)
	  TNT1 A 0 A_SpawnItem("HGCaseSpawn", 0, 20, 0)
	  
	  IRIS P 2 A_FaceTarget
	  TNT1 A 0 A_JumpIfInTargetInventory("PassClaymore", 1, "See")
	  TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "CheckRangeToWait")
	  TNT1 E 0 A_custommissile("bia_gunflash", 35, 0) 
      IRIS P 1 Bright A_CustomBulletAttack(2, 2, 1, 5, "friendbullet") 
	  TNT1 F 0 A_PlaySound("FSMG1/shot") 
	  TNT1 A 0 A_Recoil(1)
	  TNT1 A 0 A_SpawnItem("HGCaseSpawn", 0, 20, 0)
	  
      Goto See
	  
	  Niceshot:
	    TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
        //PLAY E 1
		TNT1 A 0 A_ChangeFlag("NODROPOFF", 1)
		TNT1 A 0 A_SpawnItem("KillMeSmall")
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "CheckRangeToWait")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_ChangeFlag("SOLID", 1)
		
	    IRIS E 1 A_FaceTarget
		TNT1 A 0 A_JumpIfInTargetInventory("PassClaymore", 1, "See")
	    TNT1 E 0 A_custommissile("bia_gunflash", 35, 0) 
	    TNT1 E 0 A_PlaySound("IRS/snipe")
        IRIS F 1 Bright A_VileAttack("snipe/shot", 120, 0, 3, 0, "friendbullet")
	    TNT1 A 0 A_SpawnItem ("HVRifleCaseSpawn", 0, 35, 0)
	    IRIS E 10 A_FaceTarget
	    Goto See
	  
	   Talk:
	     TNT1 A 0 A_PlaySound("IRS/shot", 2)
	     Goto see
	  
	AttackMelee:
        TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
        //PLAY E 1
		TNT1 A 0 A_ChangeFlag("NODROPOFF", 1)
		TNT1 A 0 A_SpawnItem("KillMeSmall")
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "CheckRangeToWait")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_ChangeFlag("SOLID", 1)
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0
		IRIS A 3 A_FaceTarget
		TNT1 A 0 A_Recoil(4)
		IRIS A 1 A_FaceTarget
		TNT1 E 0 A_custommissile("bia_gunflash", 35, 0) 
		 TNT1 F 0 A_PlaySound("IRS/Snipe") //
		IRIS F 1 Bright A_CustomBulletAttack(1, 1, 1, 30, "friendbullet") 
		TNT1 A 0 ThrustThing (random(0, 255), random(2, 8))
	    TNT1 A 0 A_Recoil(3)
	    IRIS E 10
	    TNT1 A 0 A_SpawnItem("HVRifleCaseSpawn", 0, 35, 0)
		Goto See

	Pain.Kick:
		IRIS A 2 A_FaceTarget
		TNT1 A 0 A_Recoil(5)
        Goto FollowPlayer

	Pain:
	    IRIS G 3
		TNT1 A 0 A_ChangeFlag("NODROPOFF", 0)
		TNT1 A 0 A_Recoil(5)
		TNT1 G 0 ThrustThing (random(0, 255), random(2, 8))
		IRIS G 3 A_Pain
		Goto FollowPlayer

	Death:
        IRIS H 4
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 0)
        IRIS I 4 A_Scream
		TNT1 A 0 //A_SpawnItemEx("RemoveIris", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
        IRIS J 4 A_Fall
        IRIS K 4
        IRIS L 4
		TNT1 A 0 A_SpawnItemEx("marine_Iris_dead", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		Stop
	}
}

ACTOR marine_Iris_dead: SwitchableDecoration
{
	Radius 10
	Height 10
	+USESPECIAL
	Speed 0
	Activation THINGSPEC_Activate
	Scale 1.0
	States
	{
	Spawn:
		IRIS N 1 Bright
		IRIS N -1 Bright
		Stop
	//Use:
	//Special:
	Active:
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_Scream
		TNT1 A 0 A_PlaySound("BERSPKUP")
		IRIS MLKJIH 3
		TNT1 A 0 //A_SpawnItemEx("ReviveIris", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		TNT1 A 0 A_SpawnItemEx("Marine_Iris_A", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		TNT1 A 1
		Stop
	}
}

ACTOR Marine_Iris_G: Marine_Iris_A
{
	FastSpeed 0
	MaxStepHeight 0
	+FRIENDLY
	+MISSILEMORE
	+NODROPOFF
	+MISSILEEVENMORE
	+PUSHABLE
	+SLIDESONWALLS
	+THRUACTORS
	Speed 0
	Health 777
	States
	{
	Active:
		TNT1 A 0 A_ChangeFlag("NOBLOCKMAP", 0)
		TNT1 A 0 A_PlaySound("IRS/ac", 2)
		TNT1 A 0 A_CustomMissile ("OrderTitle1", 50, 0, 0, 2, 90)
		TNT1 A 0 A_GiveInventory("PassClaymore", 1)
		TNT1 A 0 A_SpawnItemEx ("Marine_Iris_A", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		Stop
		
	Spawn:
