		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "FollowPlayer")
		TNT1 A 0 A_JumpIfCloser(300, "Waits")
		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
		
		RAN6 CC 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "FollowPlayer")
		TNT1 A 0 A_JumpIfCloser(300, "Waits")
		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
		
		RAN6 DD 1 A_Chase
		TNT1 A 0 A_Recoil(-1)
		
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "FollowPlayer")
		TNT1 A 0 A_JumpIfCloser(300, "Waits")
		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
	
		TNT1 A 0 A_JumpIfCloser(300, "Waits")
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "FollowPlayer")
		Goto See
	BecomeEnemy23:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		RAN6 A 1
		TNT1 A 0 A_ClearTarget
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		Goto FollowPlayer
		
	FollowPlayer:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 1)
	    TNT1 A 0 A_ChangeFlag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("FASTER", 1)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_GiveToTarget("NumberOfMarineReznov", 1)
	    TNT1 A 0 A_JumpIfCloser(200, "Waits")
		TNT1 A 0 A_ChangeFlag("MISSILEMORE", 0)
		TNT1 A 0 A_ChangeFlag("MISSILEEVENMORE", 0)
		RAN6 A 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		TNT1 A 0 A_Recoil(-1)
		RAN6 AA 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		RAN6 BB 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		RAN6 BB 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		RAN6 CC 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		RAN6 CC 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		RAN6 DD 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		RAN6 DD 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		
		RAN6 AA 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		RAN6 BB 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		RAN6 BB 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		RAN6 CC 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
		TNT1 A 0 A_Recoil(-1)
		RAN6 CC 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		RAN6 DD 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		RAN6 DD 1 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("Followcall", 1, "Pathfind")
		TNT1 A 0 A_Recoil(-1)
		
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 1)
		TNT1 A 0 A_JumpIfTargetInLOS("See")
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 0)

		TNT1 A 0 A_CheckSight("CheckIfPlayerSee")
		Loop
	
	CheckRangeToWait:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0
		TNT1 A 0 A_JumpIfCloser(200, "Waits")
		Goto FollowPLayer
	
	CheckIfPlayerSee:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0
		RAN6 AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA 0 A_FastChase
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		////TNT1 A 0 Thing_Hate(94, 0, 0)
		TNT1 A 0 A_JumpIfCloser(800, 2)
		Goto Pathfind
		TNT1 AAA 0
		Goto FollowPlayer

	Pathfind:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_JumpIFInTargetInventory("IsPLayer", 1, 1)
		Goto FollowPLayer
		//TNT1 A 1 A_Chase("", "")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFlag("NOBLOCKMAP", 1)
		TNT1 A 0 A_ChangeFlag("NOCLIP", 1)
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
		TNT1 A 0 A_Noblocking
		TNT1 A 0 A_SpawnItem("TeleportFog")
		TNT1 A 0 A_SpawnItemEx ("Marine1Pathfinder_Reznov", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		Stop

	Waits:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		RAN6 E 1
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, 2)
		TNT1 A 0 A_ClearTarget
		Goto See
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("MISSILEMORE", 1)
		TNT1 A 0 A_ChangeFlag("MISSILEEVENMORE", 1)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
		TNT1 A 0 A_GiveToTarget("NumberOfMarineReznov", 1)
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "BecomeEnemy23")
		TNT1 A 0 A_Jump(32, "Waits2")
		TNT1 A 0 A_Jump(32, "Waits3")
		
		RAN6 E 8 A_Stop
		RAN6 E 8 A_Stop
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_Stop
		TNT1 A 0 A_CheckSight("FollowPlayer")
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 1)
		TNT1 A 0 A_ClearTarget
		
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		RAN6 E 0 A_Look
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		RAN6 E 1 A_LookEx(0, 0, 0, 0, 0, "FollowPlayer")
		TNT1 A 0 A_CheckSIght("FollowPlayer")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_JumpIfCloser(300, "Waits")
		Goto FollowPlayer
	
	Waits2:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0 A_SetAngle(random(90, -90) + angle)
		RAN6 E 8 A_Stop
		RAN6 E 8 A_Stop
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_CheckSight("FollowPlayer")
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 1)
		TNT1 A 0 A_ClearTarget
		
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		RAN6 E 0 A_Look
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		RAN6 E 1 A_LookEx(0, 0, 0, 0, 0, "FollowPlayer")
		TNT1 A 0 A_CheckSIght("FollowPlayer")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_JumpIfCloser(400, "Waits")
		Goto FollowPLayer
	
	Waits3:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 AAA 0
		TNT1 A 0 A_SetAngle(random(90, -90) + angle)
		RAN6 E 8 A_Stop
		RAN6 E 8 A_Stop
		TNT1 A 0 A_JumpIfInTargetInventory("Ammocall", 1, "GiveAmmo")
		TNT1 A 0 A_JumpIfInTargetInventory("Stimcall", 1, "GiveStim")
		TNT1 A 0 A_CheckSight("FollowPlayer")
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 1)
		TNT1 A 0 A_ClearTarget
		
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		RAN6 E 0 A_Look
		TNT1 A 0 A_ChangeFLag("FRIENDLY", 0)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		RAN6 E 1 A_LookEx(0, 0, 0, 0, 0, "FollowPlayer")
		TNT1 A 0 A_CheckSIght("FollowPlayer")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_JumpIfCloser(400, "Waits")
		Goto FollowPLayer
	
	GiveAmmo:
	    TNT1 A 0 A_TakefromTarget("FragGrenadeAmmo", 1)	
		RAN6 E 30 A_FaceTarget
		TNT1 E 0 A_CustomMissile ("FriendGiveAmmo_45ACP", 32, 0, 0, 0, 35) 
		RAN6 E 30
		Goto Waits
		
	GiveStim:
	    TNT1 A 0 A_TakefromTarget("StunGrenadeInventory", 1)	
		TNT1 A 0 A_TakefromTarget("Stimcall", 1)
		RAN6 E 30 A_FaceTarget
		TNT1 E 0 A_CustomMissile ("FriendGiveStimpack", 32, 0, 0, 0, 35) 
		RAN6 E 30
		Goto Waits
	
	Missile:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
        //PLAY E 1
		TNT1 A 0 A_ChangeFlag("NODROPOFF", 1)
		TNT1 A 0 A_SpawnItem("KillMeSmall")
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "CheckRangeToWait")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_ChangeFlag("SOLID", 1)
		TNT1 E 0 //A_Jump(3, "Talk")
		
	  RAN6 E 1 A_FaceTarget
	  TNT1 A 0 A_JumpIfInTargetInventory("PassClaymore", 1, "See")
	  TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "CheckRangeToWait")
	   TNT1 A 0 A_Recoil(1)
	  TNT1 A 0 A_AlertMonsters
      RAN6 E 1 Bright A_CustomBulletAttack (2, 2, 1, 6, "friendbullet") 
	  TNT1 A 0 A_SpawnItem ("556RifleCaseSpawn", 0, 40, 0)
	  TNT1 E 0 A_custommissile("bia_gunflash", 40, 0) 
	  TNT1 A 0 ThrustThing (random(0, 255), random(2, 8))
      RAN6 E 1 
	  
	  RAN6 E 1 A_FaceTarget
	  TNT1 A 0 A_JumpIfInTargetInventory("PassClaymore", 1, "See")
	  TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "CheckRangeToWait")
	   TNT1 A 0 A_Recoil(1)
	  TNT1 A 0 A_AlertMonsters
      RAN6 E 1 Bright A_CustomBulletAttack (2, 2, 1, 6, "friendbullet") 
	  TNT1 A 0 A_SpawnItem ("556RifleCaseSpawn", 0, 40, 0)
	  TNT1 E 0 A_custommissile("bia_gunflash", 40, 0) 
	  TNT1 A 0 ThrustThing (random(0, 255), random(2, 8))
      RAN6 E 1 
	  
	  RAN6 E 1 A_FaceTarget
	  TNT1 A 0 A_JumpIfInTargetInventory("PassClaymore", 1, "See")
	  TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "CheckRangeToWait")
	   TNT1 A 0 A_Recoil(1)
	  TNT1 A 0 A_AlertMonsters
      RAN6 E 1 Bright A_CustomBulletAttack (2, 2, 1, 6, "friendbullet") 
	  TNT1 A 0 A_SpawnItem ("556RifleCaseSpawn", 0, 40, 0)
	  TNT1 E 0 A_custommissile("bia_gunflash", 40, 0) 
	  TNT1 A 0 ThrustThing (random(0, 255), random(2, 8))
      RAN6 E 1 
	  
      Goto See
	  
	Shotchange:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
        //PLAY E 1
		TNT1 A 0 A_ChangeFlag("NODROPOFF", 1)
		TNT1 A 0 A_SpawnItem("KillMeSmall")
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "FollowPLayer")//
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "CheckRangeToWait")
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_ChangeFlag("SOLID", 1)
		RAN6 E 2 A_FaceTarget
		TNT1 A 0 A_JumpIfInTargetInventory("PassClaymore", 1, "See")
	  TNT1 A 0 A_AlertMonsters
      RAN6 E 1 Bright A_CustomBulletAttack (1, 1, 1, 70, "friendbullet") 
	  TNT1 A 0 A_PlaySound("weapons/knifehits", 2)
	   TNT1 A 0 A_Recoil(2)
	   TNT1 A 0 ThrustThing (random(0, 255), random(2, 8))
	  RAN6 E 1 A_CPosRefire
      Goto Missile

	  Talk:
	     TNT1 A 0 A_PlaySound("USC/talk", 2)
	     Goto see

	AttackMelee:
	TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0
		RAN6 A 4 A_FaceTarget
		TNT1 A 0 A_Recoil(-4)
		RAN6 A 1 A_FaceTarget
		RAN6 E 4 A_CustomMissile("MarinePunch", 40, 0, 0, 0)
		RAN6 A 2
		Goto See

	Pain.Kick:
		RAN6 A 2 A_FaceTarget
		TNT1 A 0 A_Recoil(5)
        Goto FollowPlayer

	Pain:
	    RAN6 G 3
		TNT1 A 0 A_ChangeFlag("NODROPOFF", 0)
		RAN6 G 3 A_Pain
		Goto FollowPlayer

	Death:
        RAN6 H 4
		TNT1 A 0 //A_CustomMissile ("MuchBlood", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 0)
        RAN6 I 4 A_Scream
		TNT1 A 0 A_SpawnItemEx("RemoveReznov", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
        RAN6 J 4 A_Fall
        RAN6 K 4
		TNT1 A 0 //A_SpawnItem ("MediumBloodSpot")
		TNT1 A 0 A_SpawnItemEx ("marine_Reznov_dead", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
        Stop
	}
}

ACTOR marine_Reznov_dead: SwitchableDecoration
{
	Radius 10
	Height 10
	+USESPECIAL
	Speed 0
	Activation THINGSPEC_Activate
	Scale 0.68
	States
	{
	Spawn:
		RAN6 L 1 Bright
		RAN6 L -1 Bright
		Stop
	//Use:
	//Special:
	Active:
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_Scream
		TNT1 A 0 A_PlaySound("BERSPKUP")
		RAN6 LKJIH 3
		TNT1 A 0 A_SpawnItemEx("ReviveReznovActivate", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		TNT1 A 0 A_SpawnItemEx("Marine_Reznov_A", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		TNT1 A 1
		Stop
	}
}

ACTOR Marine_Reznov_G: Marine_Reznov_A
{
	FastSpeed 0
	MaxStepHeight 0
	+FRIENDLY
	+MISSILEMORE
	+NODROPOFF
	+MISSILEEVENMORE
	+THRUACTORS
	Speed 0
	States
	{
	Active:
		TNT1 A 0 A_ChangeFlag("NOBLOCKMAP", 0)
		TNT1 A 0 A_PlaySound("USC/ac", 2)
		TNT1 A 0 A_CustomMissile ("OrderTitle1", 50, 0, 0, 2, 90)
		TNT1 A 0 A_GiveInventory("PassClaymore", 1)
		TNT1 A 0 A_SpawnItemEx ("Marine_Reznov_A", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION, 0)
		Stop
		
	Spawn:
		TNT1 A 0 A_GiveInventory("PassClaymore", 1)
		TNT1 A 0
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 1)
		TNT1 A 0 A_GiveInventory("TargetIsAMarine")
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		RAN6 E 20
		RAN6 E 1 A_Look
		Goto See
		
	See:	
		//TNT1 A 0 A_SetAngle(random(90, -90) + angle)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_ChangeFlag("SOLID", 0)
		RAN6 EEEEEEEEEEEEEEEE 1 A_Chase
		TNT1 A 0 A_ClearTarget
		Loop

	Missile:
        //PLAY E 1
		TNT1 A 0 A_ChangeFlag("FRIENDLY", 1)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 1)
		TNT1 A 0 A_ChangeFlag("SOLID", 1)
		TNT1 A 0 A_ChangeFlag("NODROPOFF", 1)
		TNT1 A 0 A_SpawnItem("KillMeSmall")
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMarine", 1, "See")
		TNT1 A 0 A_JumpIfInTargetInventory("IsPlayer", 1, "See")
		
      RAN6 E 1 A_FaceTarget
